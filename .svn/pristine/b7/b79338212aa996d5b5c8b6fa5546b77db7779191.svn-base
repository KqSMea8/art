html, body {
  height: 100%;
}
body {
  background: #f4f4f4;
}
.icons {
  display: inline-block;
  background-repeat: no-repeat;
  -webkit-background-size: 13.33333rem 13.33333rem;
  background-size: 13.33333rem 13.33333rem;
  background-image: url(../image/home/pc-icons.png);
}
/*头部*/
.y-header {
  position: relative;
  height: 1.0rem;
  background-color: #fff;
  border-bottom: 1px solid #e2e2e2;
}
.y-head {
  margin-top: 0.16rem;
  margin-left: 0.37333rem;
}
.y-head .logo {
  height: 0.66667rem;
  width: 1.73333rem;
  vertical-align: middle;
}
.y-title {
  margin-left: 0.2rem;
  margin-top: 0.06667rem;
  padding-left: 0.13333rem;
  height: 0.53333rem;
  line-height: 0.53333rem;
  font-size: 0.4rem;
  vertical-align: middle;
  color: #999;
  border-left: 2px solid #e0e0e0;
}


/*底部*/
.y-footer {
  position: relative;
  padding: 0.29333rem;
  text-align: center;
  font-size: 0.21333rem;
  line-height: 1.5;
  color: #8f8f8f;
  background-color: #e9e9e9;
}
/*第一屏*/
.main-1 {
  position: relative;
  width: 100%;
  margin: 0 auto;
  height: 6.37333rem;
  background-image: url(https://artzhe.oss-cn-shenzhen.aliyuncs.com/static/mobile/home/banner.png);
  -webkit-background-size: cover;
  background-size: cover;
}
.main-1 .gotohome {
  width: 100%;
  height: 100%;
}
.main-1 .slogan {
  position: relative;
  margin-top: 2.0rem;
  font-size: 0.64rem;
  line-height: 1.33333rem;
  color: #fff;
}
.slogan .s1,
.slogan .s2 {
  text-align: center;
}

.main-1 .slogan:before {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -0.53333rem;
  margin-top: -1px;
  width: 1.06667rem;
  height: 0.04rem;
  background-color: #f4ad23;
}
.main-1 .btn-group {
  margin-top: 1.8rem;
  text-align: center;
}
.main-1 .btn,
.main-3 .btn {
  display: inline-block;
  width: 6.0rem;
  height: 1.25333rem;
  font-size: 0.4rem;
  line-height: 1.25333rem;
  color: #333;
  -webkit-box-shadow: 0 0 0 #f4ad23;
  box-shadow: 0 0 0 #f4ad23;
}
/*第一屏 end*/


/*第二屏*/
.main-2 {
  position: relative;
  padding-top: 0.77333rem;
  height: 18.90667rem;
  background-color: #f9f6f1;
}
.main-2 .swipe {
  position: relative;
  margin: 0 auto;
  padding-top: 0.53333rem;
  width: 7.93333rem;
  height: 9.6rem;
  overflow: hidden;
  background-color: #666;
  box-shadow: 0px 0.18667rem 0.32rem rgba(72, 48, 9, 0.2);
}
#swiper-container-1 .swiper-slide {
  margin: 0 0.08rem;
  width: 6.8rem;
  height: 8.2rem;
}
.swiper-slide.swiper-slide-prev .inner, .swiper-slide.swiper-slide-next .inner {
  transform: scale(0.95);
}
.swiper-container-horizontal>.swiper-pagination-bullets {
  text-align: center;
}
.swiper-pagination-bullet {
  width: 0.12rem;
  height: 0.12rem;
  display: inline-block;
  border-radius: 100%;
  background: transparent;
  border: 1px solid #f4ad23;
}
.swiper-pagination-bullet-active {
  background-color: #f4ad23;
}
.main-2 .inner img {
  width: 6.8rem;
  height: 4.06667rem;
  border-radius: 0.08rem;
}
.main-2 .inner .info {
  position: relative;
  margin: 0.42667rem auto 0;
  width: 5.06667rem;
  color: #fff;
}
.main-2 .inner .icons {
  position: absolute;
  width: 0.66667rem;
  height: 0.66667rem;
}
.main-2 .inner .icons.icon-quote1 {
  left: -0.66667rem;
  top: -0.16rem;
  background-position: -2.66667rem -1.06667rem;
}
.main-2 .inner .icons.icon-quote2 {
  right: -0.6rem;
  bottom: -0.4rem;
  background-position: -3.33333rem -1.06667rem;
}
.main-2 .inner .info h3 {
  font-size: 0.37333rem;
  line-height: 0.6rem;
}
.main-2 .inner .info p {
  font-size: 0.26667rem;
  line-height: 0.4rem;
}
.main-2 .mission {
  margin: 0.53333rem auto 0;
  width: 6.08rem;
}
.main-2 .mission h2 {
  position: relative;
  padding-bottom: 0.76rem;
  line-height: 1;
  font-size: 0.42667rem;
  font-weight: bold;
  color: #333;
  white-space: nowrap;
}
.main-2 .mission h2:before {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 1.2rem;
  border-bottom: 0.04rem solid #f4ad23;
}
.main-2 .mission ul {
  padding-top: 0.66667rem;
  font-size: 0.26667rem;
  line-height: 0.4rem;
  color: #555;
}
.main-2 .mission ul li {
  position: relative;
  margin-bottom: 1.6rem;
}
.mission ul .icons {
  position: absolute;
  left: -1.06667rem;
  top: 0;
  width: 0.8rem;
  height: 0.8rem;
}
.icons.icon-yun {
  background-position: -1.86667rem 0;
}
.icons.icon-hua {
  background-position: -2.8rem 0;
}
.icons.icon-wang {
  background-position: -3.6rem 0;
}
/*第二屏 end*/

/*第三屏*/
.main-3 {
  position: relative;
  padding-top: 1.06667rem;
  height: 5.70667rem;
  background-color: #fff;
  box-shadow: 0px 0.06667rem 0.13333rem rgba(66, 46, 12, 0.1);
  z-index: 1;
}
.main-3 .art-mobile {
  width: 10.0rem;
}
.main-3 .art-mobile h2 {
  margin-bottom: 0.69333rem;
  text-align: center;
  font-size: 0.42667rem;
  font-weight: bold;
  color: #444;
}
.main-3 .art-mobile h2 span {
  display: inline-block;
  position: relative;
  padding-left: 0.24rem;
  font-size: 0.32rem;
  font-weight: normal;
}
.art-mobile h2 span i {
  position: absolute;
  right: -0.64rem;
  bottom: 0.24rem;
  padding: 0.05333rem 0.12rem;
  font-size: 0.16rem;
  line-height: 1;
  color: #666;
  background-color: #f9f6f1;
  border: 1px solid #bfbfbf;
  border-radius: 0.05333rem;
}
.art-mobile dl {
  float: left;
  margin: 0 0.4rem 0 1.0rem;
}
.art-mobile dl dt {
  position: relative;
  margin-bottom: 0.18667rem;
  padding-left: 0.12rem;
  height: 0.45333rem;
  line-height: 0.45333rem;
  font-size: 0.4rem;
  color: #444;
}
.art-mobile dl dt:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 0.32rem;
  height: 0.45333rem;
  background-color: #f5b230;
}
.art-mobile dl dt h4 {
  position: relative;
}
.art-mobile dl dd {
  line-height: 0.4rem;
  font-size: 0.24rem;
  color: #888;
}
.main-3 .btn-group {
  margin-top: 0.66667rem;
  text-align: center;
}
/*第三屏 end*/

/*第四屏*/
.main-4 {
  position: relative;
  padding-top: 0.8rem;
  height: 6.08rem;
  background-color: #f9f6f1;
  box-shadow: 0px 0.04rem 0.13333rem rgba(66, 46, 12, 0.1);
}
.main-4 .team-info {
  position: relative;
  margin: 0 auto;
  width: 8.88rem;
  /*height: 345px;*/
  color: #656565;
}
.main-4 .team-info h2 {
  margin-bottom: 0.26667rem;
  font-size: 0.4rem;
  line-height: 1;
  font-weight: bold;
  text-align: center;
}
.main-4 .team-info p {
  font-size: 0.24rem;
  line-height: 0.34667rem;
}
.main-4 .team-info .address {
  margin-top: 0.4rem;
}
.main-4 .team-info .address h3,
.main-4 .team-info .address p {
  font-size: 0.24rem;
  line-height: 0.4rem;
}
.main-4 .team-info .address h3 {
  font-weight: bold;
}
/*第四屏 end*/
.icons.icon-downloadapp-s {
  display: inline-block;
  vertical-align: middle;
  background-size: 12.0rem 12.0rem;
  background-repeat: no-repeat;
  background-image: url(../image/icons/icons.png);
}