html, body {
  height: 100%;
}
body {
  background: #f4f4f4;
}

/*步骤*/
.stage-wrap {
  padding-bottom: 72px;
  color: #333;
}
.navul {
  position:relative;
  margin:126px 0 30px 285px;

  /*width:822px;*/
}
.navul li {
  float:left;
  position: relative;
  height: 38px;
  padding-right: 90px;
  font-size: 18px;
  text-align:center;
  color:#999;
}
.navul li::before {
  content: '';
  position: absolute;
  top: 18px;
  left: -77px;
  width: 64px;
  border-bottom: 2px solid #d8d8d8;
}
.navul li:first-of-type::before {
  width: 0;
}
.navul li .i-num {
  display:inline-block;
  margin-right:6px;
  width:38px;
  height:38px;
  line-height:38px;
  text-align:center;
  font-size: 20px;
  font-family:arial;
  border-radius:50%;
  color:#fff;
  background-color:#d2d2d2;
}
.navul li.active {
  color:#444;
}
.navul li.active::before {
  border-bottom: 2px solid #f4bd52;
}
.navul li.active .i-num {
  background-color:#f4ad23;
}


/*表单样式*/


.reg-wrap {
  margin: 0 auto;
  padding-top: 60px;
  width: 730px;
  border: 1px solid #dcdcdc;
}
.form .item {
  font-size: 18px;
  color: #333;
}
.form .item .tit {
  width: 215px;
  margin-right: 12px;
  text-align: right;
  float: left;
  line-height: 48px;
}
.form .item .con {
  position: relative;
  float: left;
  width: 350px;
}

.input {
  display: block;
  width: 100%;
  height: 48px;
  padding: 6px 12px;
  font-size: 18px;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text {
  display: block;
  width: 100%;
  padding: 12px;
  font-size: 16px;
  line-height: 1.5;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text:focus,.input:focus {
  border-color: #f4ad23;
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
}

.form .item .input-m {
  display: inline-block;
  width: 202px;
}
.form .item .tip {
  height: 30px;
  padding: 0 1px;
}
.Validform_checktip {
  height: 20px;
  line-height: 20px;
  display: block;
  width: 100%;
  color: #e82c3a;
  font-size: 14px;
  background: none;
  padding: 0;
  margin: 0;
}

.next {
  padding: 30px 0 75px;
}
.next-2 {
  padding: 30px 0 80px;
}

.imgcode {
  margin-left: 12px;
  width: 85px;
  height: 44px;
  vertical-align: top;
}
.btn-s {
  margin: 6px;
  padding: 3px 18px;
  height: 36px;
  border: 1px solid #b6b6b6;
  border-radius: 2px;
  background-color: #fff;
  cursor: pointer;
}

.mark {
  font-size: 14px;
  color: #9f9f9f;
}

.input.input-35 {
  width: 350px;
}


.third-wrap .item {
  text-align: center;
  color: #666;
}
.third-wrap .msg {
  padding: 20px;
  font-size: 30px;
  line-height: 1;
}
.third-wrap .back {
  font-size: 16px;
  vertical-align: bottom;
}
.third-wrap .back span {
  font-size: 20px;
  color: #5495d1;
}
/*表单样式 end*/
.preview {
  height: 82px;
  background-color: #fff;
  border-top: 1px solid #dedede;
  border-bottom: 1px solid #dedede;
}
.preview .btn-s {
  margin-top: 20px;
  height: 40px;
}

/*步骤 end */