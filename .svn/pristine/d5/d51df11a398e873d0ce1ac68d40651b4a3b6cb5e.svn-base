html, body {
  height: 100%;
}
body {
  background: #f4f4f4;
}
.el-upload__tip {
  display: inline-block;
  padding-left: 5px;
  font-size: 14px;
  color: #9f9f9f;
}
/*auth 获取手机验证码*/
.verify .el-form-item__content {
  width: 350px;
}

/*规则*/
.rule-wrap .rule {
  margin-bottom: 32px;
  font-size: 16px;
  color: #666;
}
.rule-wrap .rule h3 {
  position: relative;
  line-height: 3;
  color: #444;
}
.rule-wrap .rule h3::before {
  content: '';
  position: absolute;
  left: -10px;
  top: 50%;
  margin-top: -3px;
  width: 6px;
  height: 6px;
  border-radius: 3px;
  background-color: #f4ad23;
}
.rule-wrap .rule p {
  line-height: 1.5;
}
.agree-bar {
  margin: 0 auto;
  width: 240px;
  text-align: center;
}
.agree-check {
  position: relative;
  padding-left: 25px;
  font-size: 16px;
  line-height: 2;
  text-align: left;
}
.agree-check input {
  display: none;
}
.agree-check .icon-agree {
  position: absolute;
  left: 0;
  top: 7px;
  display: inline-block;
  width: 18px;
  height: 18px;
  background-position: -1px -1px;
}
.agree-check .icon-agreed {
  position: absolute;
  left: 0;
  top: 7px;
  display: inline-block;
  width: 18px;
  height: 18px;
  background-position: -1px -19px;
}

/* 规则 end */

/*步骤*/
.stage-wrap {
  color: #333;
}
.stage-wrap .info {
  position: relative;
  padding: 80px 0 75px;
  line-height: 21px;
  font-size: 18px;
  text-align: center;
}
.icons.icon-info {
  width: 21px;
  height: 21px;
  vertical-align: top;
  background-position: 0 -40px;
}

/*表单样式*/
.first-wrap {
}
.form .item {
  font-size: 18px;
  color: #333;
}
.form .item .tit {
  width: 445px;
  margin-right: 12px;
  text-align: right;
  float: left;
  line-height: 48px;
}
.form .item .con {
  position: relative;
  float: left;
  width: 350px;
}

.input {
  display: block;
  width: 100%;
  height: 48px;
  padding: 6px 12px;
  font-size: 18px;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text {
  display: block;
  width: 100%;
  padding: 12px;
  font-size: 16px;
  line-height: 1.5;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text:focus,.input:focus {
  border-color: #f4ad23;
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
}

.form .item .input-m {
  display: inline-block;
  width: 202px;
}
.form .item .tip {
  height: 30px;
  padding: 0 1px;
}
.Validform_checktip {
  height: 20px;
  line-height: 20px;
  display: block;
  width: 100%;
  color: #e82c3a;
  font-size: 14px;
  background: none;
  padding: 0;
  margin: 0;
}

.next {
  padding: 30px 0 80px;
}
.next-2 {
  padding: 30px 0 80px;
}

.btn-s {
  margin: 6px;
  padding: 3px 18px;
  height: 36px;
  border: 1px solid #b6b6b6;
  border-radius: 2px;
  background-color: #fff;
  cursor: pointer;
}

.mark {
  font-size: 14px;
  color: #9f9f9f;
}

.second-wrap .item .con {
  width: 435px;
}

.input.input-35 {
  width: 350px;
}

/*表单样式 end*/
.preview {
  height: 82px;
  background-color: #fff;
  border-top: 1px solid #dedede;
  border-bottom: 1px solid #dedede;
}
.preview .btn-s {
  margin-top: 20px;
  height: 40px;
}

/*步骤 end */