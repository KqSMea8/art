.edit-cover{display: inline-block;}
.edit-coverTip{font-size: 15px;color: #9f9f9f;}
.edit-coverwrap{position: relative;}
.edit-coverR{position: absolute;left: 90px;top:0;}

.editBtnWrap{position: fixed;text-align: center; bottom:0;z-index: 10000;padding-top: 16px; left: 0;width: 100%;height: 80px;background-color: #fff;border-top:1px solid #dedede;border-bottom: 1px solid #dedede}
