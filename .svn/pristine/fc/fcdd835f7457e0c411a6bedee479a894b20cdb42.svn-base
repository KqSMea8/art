html, body {
  height: 100%;
  background-color: #f7f7f7;
}
#header {
  position: relative;
}

/* 最新作品 */

/* 轮播切换点 */
.swiper-pagination-bullet {
  width: 0.16rem;
  height: 0.16rem;
  border: 1px solid #eaab32;
  background-color: transparent;
  opacity: 1;
}
.swiper-pagination-bullet-active {
  background-color: #eaab32;
}

/* 轮播切换点 end */

/* 作品描述 */
.painting-new {
  position: relative;
  width: 10.0rem;
}
.painting-new .cover {
  position: relative;
  width: 10.0rem;
  height: 10.0rem;
}
.painting-new .cover img {
  display: block;
  width: 100%;
  height: 100%;
}
.painting-new .cover .icon-update {
  position: absolute;
  right: 0.36rem;
  top: 0.2rem;
}
.painting-new .detail {
  position: relative;
  padding: 0 0.4rem;
  background-color: #fff;
}
.painting-new .detail .title {
  height: 0.90667rem;
  vertical-align: middle;
}
.painting-new .title h3 {
  float: left;
  margin-top: 0.24rem;
  height: 0.42667rem;
  max-width: 5.0rem;
  line-height: 0.42667rem;
  font-size: 0.37333rem;
  vertical-align: middle;
  color: #333;
}
.painting-new .detail .label {
  color: #999;
}
.painting-new .title .icon-r {
  float: right;
  margin-top: 0.24rem;
  margin-left: 0.46667rem;
  vertical-align: middle;
}
.painting-new .title .label {
  display: inline-block;
  height: 0.4rem;
  line-height: 0.4rem;
  vertical-align: middle;
  font-size: 0.24rem;
  color: #999;
}
.painting-new .title .icons {
  width: 0.53333rem;
  height: 0.53333rem;
}
.icons.icon-eye-grey {
  background-position: -10.26667rem -4.24rem;
}
.icons.icon-heart-grey {
  background-position: -9.28rem -4.24rem;
}
.painting-new .flags {
  height: 0.96rem
}
.icons.icon-flag {
  width: 0.48rem;
  height: 0.96rem;
  background-position: -0.24rem -8.0rem;
}
.painting-new .flags .label {
  display: inline-block;
  margin: 0.24rem 0.06rem 0;
  padding: 0.09333rem 0.21333rem;
  font-size: 0.26667rem;
  line-height: 1;
}

/* 作品描述 end */

/* 作者信息 */
.painting-new .painter {
  position: relative;
  margin-top: 0.22rem;
  padding: 0 0.4rem;
  background-color: #fff;
}
.painting-new .painter .desc {
  padding: 0.13333rem 0;
  line-height: 0.64rem;
  font-size: 0.4rem;
  color: #333;
  border-bottom: 1px solid #eee;
}
.painting-new .painter .desc p {
  font-size: 0.32rem;
}
.painter .info {
  position: relative;  /*height: 1.54667rem;*/
}
.painter .info .avatar {
  position: absolute;
  top: 50%;
  margin-top: -0.58667rem;
  width: 1.17333rem;
  height: 1.17333rem;
  border-radius: 50%;
  box-shadow: 0 0.08rem 0.18667rem rgba(0, 0, 0, .15);
}
.painter .info .det {
  padding: 0.3rem 0 0.3rem 1.6rem;
}
.info .det .name {
  height: 0.74667rem;
  line-height: 0.74667rem;
  font-size: 0.48rem;
  color: #333;
}
.info .det span {
  display: inline-block;
  padding-right: 0.2rem;
  height: 0.58667rem;
  line-height: 0.58667rem;
  font-size: 0.32rem;
  color: #666;
}
.painter .info .follow {
  position: absolute;
  top: 50%;
  right: 0;
  height: 1.2rem;
  margin-top: -0.6rem;
}
.painter .info .follow .label {
  line-height: 0.53333rem;
  font-size: 0.32rem;
}
.painter .info .follow.followed {
  color: #eaab32;
}
.icons.icon-follow {
  display: block;
  margin: 0 auto;
  width: 0.66667rem;
  height: 0.66667rem;
  background-position: -0.16rem -7.16rem;
}
.followed .icons.icon-follow {
  display: block;
  margin: 0 auto;
  width: 0.66667rem;
  height: 0.66667rem;
  background-position: -1.16rem -6.16rem;
}

/* 作者信息 end */

/* 最新作品 end */

/* 留言板 */
.message-board {
  position: relative;
  margin-top: 0.16rem;
  padding: 0 0.4rem;
  background-color: #fff;
}
.message-board .title {
  padding: 0.56rem 0;
  font-size: 0.42667rem;
  line-height: 1;
  color: #333;
}
.message-board .user-list {
  margin-right: 0.24rem;
}
.message-board .user-list .avatar {
  float: right;
  margin: 0.49rem 0 0 -0.16rem;
  width: 0.56rem;
  height: 0.56rem;
  border: 1px solid #ccc;
  border-radius: 50%;
}
.message-board .icons {
  position: absolute;
  right: 0.3rem;
  top: 50%;
  margin-top: -0.16rem;
  height: 0.4rem;
  width: 0.32rem;
  background-position: -2.4rem -5.33333rem;
}

/* 留言板 end */

/* 创作过程 */
.updates {
  position: relative;
  margin-top: 0.16rem;
  padding-bottom: 2.66rem;
}
.update-list {
  padding-top: 0.2rem;
}
.updates .title {
  padding: 0.34667rem 0.4rem;
  font-size: 0.42667rem;
  line-height: 1;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}
.updates .title .year {
  position: relative;
  margin-left: 0.16rem;
  padding-left: 0.13333rem;
  font-size: 0.37333rem;
  line-height: 1;
  color: #666;
}
.updates .title .year:before {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -0.13333rem;
  height: 0.26667rem;
  width: 1px;
  background-color: #d8d8d8;
}
.update-year-item .year {
  top: 0;
  width: 100%;
  padding-left: 1.08rem;
  font-size: 0.64rem;
  line-height: 0.88rem;
  background-color: #f7f7f7;
  z-index: 1;
}
.updates .update-detail-item {
  position: relative;
  margin-top: 0.4rem;
  margin-left: 1.08rem;
  padding-bottom: 0.66667rem;
  width: 8.56rem;
}
.update-detail-item .icons.icon-timeline {
  position: absolute;
  left: -0.82rem;
  top: 0;
  height: 0.69333rem;
  width: 0.69333rem;
  background-position: -1.17333rem -8.16rem;
}
.update-detail-item:before {
  content: '';
  position: absolute;
  left: -0.53333rem;
  top: 0.72rem;
  width: 0.08rem;
  height: 120%;
  background-color: #e4e4e4;
}
.update-year-item:last-of-type .update-detail-item:last-of-type:before {
  left: -0.56rem;
  width: 0.15rem;
  top: -0.04rem;
  height: 80%;
  background-color: #f7f7f7;
}
.icons.icon-ring {
  position: absolute;
  top: 0.34rem;
  left: 0.26667rem;
  width: 0.53333rem;
  height: 1.6rem;
  background-position: -1.49333rem -8.93333rem;
  z-index: 10;
}
.update-detail-item .info {
  position: relative;
}
.update-detail-item .detail {
  position: relative;
  margin-top: 0.16rem;
  background-color: #fff;
  border: 1px solid #eee;
  border-radius: 0.04rem;
}
.detail .icons.icon-smallarrow {
  position: absolute;
  top: -0.18rem;
  left: 0.48rem;
  width: 0.26667rem;
  height: 0.26667rem;
  background-position: -2.37333rem -8.36rem;
}
.update-detail-item .detail .desc {
  margin: 0 0.24rem;
  padding-bottom: 0.21333rem;
  font-size: 0.37333rem;
  line-height: 0.48rem;
  color: #666;
  border-bottom: 1px solid #e9e9e9;
}
.update-detail-item .date {
  height: 0.66667rem;
  color: #444;
}
.update-detail-item .date .day {
  font-size: 0.50rem;
  font-weight: 700;
}
.update-detail-item .date .month {
  margin-left: -0.04rem;
  font-size: 0.32rem;
}
.update-detail-item .update-num {
  float: right;
  vertical-align: middle;
}
.update-detail-item .update-num .label {
  display: inline-block;
  font-size: 0.32rem;
  line-height: 0.66667rem;
  vertical-align: middle;
}

/* 图片排版 */
.img-wrap {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  width: 8.05333rem;
  margin: 0.26667rem auto 0.13333rem;
  justify-content: flex-start;
}
[class*="img-item-"] {
  margin-bottom: 0.13333rem;
}
.img-item-1 {
  width: 4.16rem;
  height: 4.16rem;
}
.img-item-2, .img-item-4 {
  width: 3.81333rem;
  height: 3.81333rem;
}
.img-item-3 {
  margin-right: 0.1rem;
  width: 2.6rem;
  height: 2.6rem;
}
.img-wrap .img-item-3:last-of-type {
  margin-right: 0;
}
/* 图片排版 end */
