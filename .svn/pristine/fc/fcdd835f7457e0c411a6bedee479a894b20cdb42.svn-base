/*新增交互栏*/

/* interact-bar-list */
.interact-bar-list {
  position: fixed;
  table-layout: fixed;
  right: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1.30667rem;
  color: #fff;
  border-top: 1px solid #e1e1e1;
  z-index: 10;
}
.interact-bar-item {
  position: relative;
  display: table-cell;
  width: 1%;
  height: 1.30667rem;
  text-align: center;
  vertical-align: middle;
  /*background: #161616;*/
  background: #fff;
}
.interact-bar-item .icons {
  position: relative;
  margin: 0 auto;
  width: 0.8rem;
  height: 0.8rem;
}
.interact-bar-item .icons, .interact-bar-item .label {
  display: inline-block;
  position: relative;
  vertical-align: top;
}
.interact-bar-item .label {
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.37333rem;
  /*color: #fff;*/
  color: #888;
}
.interact-bar-item.active .label {
  /*color: #eaab32;*/
}
.interact-bar-item .icon-com {
  /*background-position: -11.1rem -7.1rem;*/
  background-position: -5.12rem -10.07rem;
}
.interact-bar-item .icon-like {
  /*background-position: -8.02rem -6.09333rem;*/
  background-position: -6.12rem -9.10rem;
}
.interact-bar-item.active .icon-like {
  /*background-position: -9.02rem -6.09333rem;*/
  background-position: -6.12rem -8.10rem;
}
.interact-bar-item.followed .icon-follow {
  background-position: -4.24rem -1.17333rem;
}
.interact-bar-item.share {
  /*background-color: #eaab32;*/
}
.interact-bar-item .icon-share {
  /*background-position: -6.14rem -6.12rem;*/
  background-position: -6.1rem -10.1rem;
}
.interact-bar-item::after {
  content: '';
  position: absolute;
  right: 0;
  top: 0.44rem;
  width: 0;
  height: 0.4rem;
  border-right: 1px solid #ebebeb;

}
.interact-bar-item:last-of-type::after {
  display: none;
}

/* interact-bar-list end */

