    .result .tag.tag6{top:1.92rem;left:5.16rem;width:2.76rem;font-size:0.56rem;line-height:0.56rem;}
    .result .tag.tag7{top:3.76rem;left:0.36rem;width:4.0rem;font-size:1.29333rem;line-height:1.29333rem;}
    .result .tag.tag8{top:3.81333rem;left:4.64rem;width:3.08rem;font-size:0.92rem;line-height:0.92rem;}
    .result .tag.tag9{top:5.08rem;left:0.96rem;width:2.76rem;font-size:0.64rem;line-height:0.64rem;}
    .result .tag.tag10{top:4.88rem;left:4.44rem;width:3.41333rem;font-size:0.76rem;line-height:0.76rem;}
    .result .tag.tag11{top:5.68rem;left:3.8rem;width:2.84rem;font-size:0.42667rem;line-height:0.42667rem;}
    .result .tag.tag12{top:6.12rem;left:0.24rem;width:2.6rem;font-size:0.6rem;line-height:0.6rem;}
    .result .tag.tag13{top:6.16rem;left:2.92rem;width:3.72rem;font-size:0.76rem;line-height:0.76rem;}
    .result .tag.tag14{top:6.96rem;left:0.88rem;width:3.28rem;font-size:1.04rem;line-height:1.04rem;}
    .user-info{position:absolute;width:100%;text-align:center;}
    .user-info .user-avatar{margin-top:0.48rem;width:1.24rem;height:1.24rem;border-radius:50%;}
    .user-info .nickname{color:#252525;font-size:0.44rem;}
    .result .said{position:absolute;top:2.6rem;left:50%;margin-left:-3.74rem;width:7.48rem;height:1.16rem;line-height:1.16rem;font-size:0.53333rem;text-align:center;}

    /*结果框样式*/

    /*结果框样式end*/

    /*分享页*/
    .btn-group{text-align:center;}
    .btn.btn-share{width:8.08rem;margin:0 auto;}
    .save{width: 10.0rem; padding:0.36rem 0 0.72rem;text-align:center;font-size:0.4rem;}
    .cover{position:fixed;top:1.0rem;left:0.97333rem;width:8.05333rem;height:10.16rem;display:none;}
    .cover img{position:relative;vertical-align:bottom;}
    .cover .img-result{width:8.05333rem;height:8.05333rem;}
    .cover .img-code{position:absolute;top:8.05333rem;left:0;width:8.05333rem;height:2.10667rem;background:#fff url(/Public/image/activity/code.png) 0 0 no-repeat;-webkit-background-size:100% 100%;background-size:100% 100%;}
    #newimg{width:100%;height:100%;position:absolute;top:0;left:0;  /*z-index: 10;*/}

    /* 弹出层 */
    .layerbox{position:absolute;  /*display: none;*/left:0;top:0;width:100%;height:100%;-webkit-animation-fill-mode:both;animation-fill-mode:both;-webkit-animation-duration:.2s;animation-duration:.2s;z-index:999;}
    .layershade{position:fixed;left:0;top:0;width:100%;height:100%;background-color:rgba(0,0,0,.6);pointer-events:auto;}
    .layerbox .share{position:fixed;left:0.78rem;width:8.62667rem;height:9.13333rem;background:url(../../image/activity/share.png?v=1.0.1) no-repeat left top;background-size:100% auto;}

