html,
body{width:100%;height:100%;color:#fff;font-family:'PingFang SC Medium','microsoft yahei','arial';}
body{background:url(/Public/image/activity/bg.png) no-repeat center;-webkit-background-size:100% 100%;background-size:100% 100%;background-attachment:fixed;}
#loading{position:fixed;top:0;left:0;width:100%;height:100%;background-color:#fff;z-index:99999;}
#loading .wrap{position:absolute;top:40%;left:50%;margin-top:-1.4rem;margin-left:-2.5rem;}
#loading .box{padding-top:1.28rem;width:5.0rem;height:2.8rem;background-color:#eee;border-radius:3px;}
.progress-bar-min{position:relative;width:4.2rem;margin:0 auto;}
.progress-bar-min .progress-bar-outer{height:0.12rem;border-radius:3px;background-color:#dadada;overflow:hidden;position:relative;vertical-align:middle;}
.progress-bar-min .progress-bar-inter{position:absolute;left:0;top:0;height:100%;background-color:#f4ad23;text-align:right;border-radius:3px;line-height:1;}
#loading p{font-size:0.4rem;line-height:0.96rem;text-align:center;color:#666;}

