/*介绍页面*/
#app {
  height: 100%;
  width: 100%;
}
#intro {
  position: relative;
  height: 100%;
  width: 100%;
  text-align: center;
  background:url(/Public/image/activity/bg-line.png) no-repeat center;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}
#intro .p1 {
  padding-top: 3.2rem;
  font-size: 0.50667rem;
  line-height: 0.6rem;
}
#intro .p2 {
  margin: 1.2rem auto 0;
  padding: 0.53333rem 0;
  width: 7.26667rem;
  font-size: 0.4rem;
  line-height: 0.53333rem;
  background:url(/Public/image/activity/p-wrap.png) no-repeat center;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}
#intro .btn {
  margin-top: 2.29333rem;
  width: 8.08rem;
}
/*介绍页面end*/

/* 测试页面 */
.title{padding-top:0.36rem;line-height:1;font-size:0.48rem;text-align:center;}
.progress{margin:0.24rem auto;text-align:center;}
.progress, .progress-text, .progress-bar{ vertical-align: top;}
.progress-text{display: inline-block; padding-left: 0.24rem; font-size:0.4rem;}
.progress-bar {display: inline-block; width:6.12rem;}
.progress-bar-outer{height:0.4rem;border-radius:3px;background-color:#dedede;overflow:hidden;position:relative;vertical-align:middle;}
.progress-bar-inter{position:absolute;left:0;top:0;height:100%;background-color:#f4ad23;text-align:right;border-radius:3px;line-height:1;}
.swiper-container{padding-top:0.06rem;text-align:center; overflow: auto;}
.image-item{position:relative;margin:0 auto;width:8.2rem;height:6.0rem;text-align:center; border:0.08rem solid #fff;margin-bottom:1.04rem;background-color:#ededed;-webkit-box-shadow:inset 0 0.08rem 0.16rem rgba(0,0,0,.28);box-shadow:inset 0 0.08rem 0.16rem rgba(0,0,0,.28);}
  .image-item .image-art{position:absolute;top:50%;left:50%;margin-left:-3.8rem;margin-top:-2.7rem;width:7.6rem;height:5.4rem;background-position:50% 50%;background-size:100% 100%;background-repeat:no-repeat;-webkit-box-shadow:inset -0.12rem 0.12rem 0.24rem rgba(0,0,0,.28);box-shadow:inset -0.12rem 0.12rem 0.24rem rgba(0,0,0,.28);}
  .image-item .artinfo{position:absolute;top:6.0rem;right:0;height:0.48rem;line-height:0.48rem;font-size:0.32rem;}
  .image-item.active{border:0.08rem solid #ffdc77;}
  .image-wrap .image-item:last-of-type {margin-bottom: 0.64rem;}
  .alpha-img{height:100%;width:0;vertical-align:middle;}

  /* 测试页面 end */

  /*结果页*/
  .logo{padding-top:0.72rem;padding-bottom:0.44rem;text-align:center;}
  .logo img{width:2.10667rem;height:0.82667rem;}
  .result{position:relative;height:8.05333rem;width:8.05333rem;margin:0 auto;overflow:hidden;}
    .result .result-img{width:8.05333rem;height:10.18667rem;}
    .btns{margin-top:1.73333rem;margin-left:0.92rem;}
    .btns .btn-group{position:relative;display:inline-block;}
    .btns .btn-group p{position:absolute;top:-1.06667rem;left:50%;margin-left:-2.2rem;width:4.4rem;height:0.8rem;line-height:0.8rem;font-size:0.34667rem;text-align:center;color:#333;background-color:#f4ad23;border-radius:4px;white-space: nowrap;}
    .btns .btn-group p:after{content:'';position:absolute;bottom:-0.10rem;left:50%;margin-left:-0.12rem;width:0;height:0;border-top:0.13333rem solid #f4ad23;border-left:0.12rem solid transparent;border-right:0.12rem solid transparent;}
    .btns .btn-group:last-of-type{margin-left:0.32rem;}
    .btn{display:inline-block;width:3.92rem;height:1.25333rem;font-size:0.42667rem;line-height:1.25333rem;text-align:center;border-radius:0.64rem;color:#252525;background-color:#fff;}
    .btn.btn-primary{background-color:#f8bb2a;}
    .friend-wrap{margin:0.72rem auto 0.4rem;padding-bottom:0.4rem;width:8.34667rem;color:#fff;border:2px solid #bcbbbd;}
    .friend-wrap .friend-title{padding:0.68rem 0;font-size:0.4rem;text-align:center;}
    .friend-wrap .friend-title span{display:inline-block;padding:0 0.12rem;color:#5bb1ee;}
    .friend-item{position:relative;float:left;width:100%;padding:0.2rem 0.36rem 0.2rem 0.44rem;}
    .friend-item .friend-info{position:relative;margin-left:1.08rem;font-size:0.4rem;}
    .friend-item .friend-info .label{padding-top:0.16rem;font-size:0.34667rem;word-wrap:nowrap; }
    .friend-item .avatar{position:absolute;top:0;left:-1.08rem;width:0.96rem;height:0.96rem;border:1px solid #fff;border-radius:50%;}
    .friend-item .friend-per{float:right;line-height:0.96rem;font-size:0.32rem;vertical-align:middle;}
    .friend-item .friend-per span{vertical-align:middle;}
    .friend-item .friend-per .per{padding-left:0.12rem;font-size:0.48rem;}

    /*结果页end*/
    .result .tag{display:block;position:absolute;text-align:center;word-break:keep-all;           /* 不换行 */white-space:nowrap;          /* 不换行 */color:#d7000f;}
    .result .tag.tag1{top:0.2rem;left:0.04rem;width:3.09333rem;font-size:0.92rem;line-height:0.92rem;}
