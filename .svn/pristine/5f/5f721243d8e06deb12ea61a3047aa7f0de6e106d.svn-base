<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="艺术者">
    <meta name="description" content="艺术者">
    <title>艺术家免费认证</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon"/>
    <!-- <link href="//cdn.bootcss.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet" /> -->
    <link href="/Public/Home/js/lib/element/index.css" rel="stylesheet">
    <link href="/Public/Home/css/global.css" rel="stylesheet">
    <link href="/Public/Home/css/gcommon.css" rel="stylesheet">
    <link href="/Public/Home/css/auth.css?v=0.0.1" rel="stylesheet">
  </head>
  <body>
    <div v-cloak id="app">
      <header class="ysz-header">
        <div class="y-header">
          <div class="w clearfix">
            <h1 class="y-head">
            <img class="logo" src="/Public/Home/image/logo.png" alt="logo">
            <span class="y-title">创作平台</span>
            </h1>
          </div>
        </div>
      </header>
      <div id="main">
        <div class="w clearfix">
          <div class="main-wrap mb-72" id="auth">
            <h2 class="title">艺术家免费认证</h2>
            <div class="stage-wrap">
              <p class="info"><i class="icons icon-info"></i>以下信息必须真实可信</p>
              <div class="second-wrap form">
                <div class="item clearfix">
                  <div class="tit">个人画廊封面</div>
                  <div class="con">
                    <div class="box" >
                    <el-upload class="upload-demo" action="https://jsonplaceholder.typicode.com/posts/" :on-preview="handlePreview" :on-remove="handleRemove" :file-list="authInfo.coverImageId" list-type="picture">
                      <input id="coverImageId" name="coverImageId" type="button" class="btn-s" value="上传">
                      <div slot="tip" class="el-upload__tip">建议像素:1024*1024</div>
                    </el-upload>
                    </div>
                    <div class="tip"><div class="Validform_checktip"></div></div>
                  </div>
                </div>
                <div class="item clearfix">


                  <div class="tit"><label for="idCardImageId">身份证照片</label></div>
                  <div class="con">
                    <div class="box" >
                      <input id="idCardImageId" name="idCardImageId" type="button" class="btn-s" value="上传">
                      <!-- <span class="mark">建议像素:1024*1024</span> -->
                    </div>
                    <div class="tip"><div class="Validform_checktip"></div></div>
                  </div>
                </div>
                <div class="item clearfix">
                  <div class="tit"><label for="certImageIds">荣誉证书</label></div>
                  <div class="con">
                    <div class="box" >
                      <input id="certImageIds" name="certImageIds" type="button" class="btn-s" value="上传">
                      <span class="mark">个人荣誉证书或奖杯图片</span>
                    </div>
                    <div class="tip"><div class="Validform_checktip"></div></div>
                  </div>
                </div>
                <div class="item clearfix">
                  <div class="tit"><label for="certImageIds">作品合影</label></div>
                  <div class="con">
                    <div class="box" >
                      <input id="certImageIds" name="certImageIds" type="button" class="btn-s" value="上传">
                      <span class="mark">您与创作作品的合影</span>
                    </div>
                    <div class="tip"><div class="Validform_checktip"></div></div>
                  </div>
                </div>
                <div class="btn-group next-2">
                  <input @click="applyArtist " class="btn btn-24" type="button" value="提交">
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="preview">
          <div class="w clearfix">
            <input class="btn-s fr" type="button" value="预览您的画廊封面">
          </div>
        </div>
      </div>
      <footer>
        <div class="y-footer">
          <div class="w clearfix">
            <p class="mail">联系邮箱：yishuzhe@gaosouyi.com</p>
            <p class="qrcode">
              <img src="/Public/Home/image/qrcode-wx.png">
              <img src="/Public/Home/image/qrcode-app.png">
            </p>
            <p>
              Copyright 2017 www.artzhe.com. All Rights Reserved<br>
              粤ICP备17041531号-1
            </p>
          </div>
        </div>
      </footer>
    </div>
  </body>
  <script src="/Public/Home/js/lib/vue.js"></script>
  <script src="/Public/Home/js/lib/element/index.js"></script>
  <script src="//cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script>
  <script src="/Public/Home/js/common.js"></script>
  <script src="/Public/Home/js/auth/third.js"></script>
</html>