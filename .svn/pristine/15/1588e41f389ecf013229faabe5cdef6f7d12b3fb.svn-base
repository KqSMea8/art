html, body {
  height: 100%;
}
body {
  background: #f4f4f4;
}
.w {
  width: 960px;
}

input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
  background-color: rgba(255,255,255, 0.2);
  background-image: none;
  color: #fff;
}

/*顶部导航栏*/
.y-header-www {
  position: relative;
  height: 60px;
  line-height: 60px;
  background-color: #363636;
}
.y-header-www .y-logo {
  float: left;
  margin-right: 21px;
  width: 80px;
  height: 60px;
}
.y-header-www .y-logo img {
  width: 80px;
  height: 32px;
  vertical-align: middle;
}
.y-header-www .head-nav {

}
.y-header-www .head-nav li {
  float: left;
  margin: 0 21px;
}
.y-header-www .head-nav a {
  color: #b5b5b5;
}
.y-header-www .head-nav a:hover,
.y-header-www .head-nav a.cur {
  color: #f4ad23;
}

.y-header-www .user {
  height: 60px;
  font-size: 16px;
  line-height: 60px;
  color: #fff;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.8);
}
.y-header-www .user .info img {
  width: 40px;
  height: 40px;
  vertical-align: middle;
  border-radius: 20px;
}
.y-header-www .user .info span {
  display: inline-block;
  margin-left: 10px;
  margin-right: 10px;
  max-width: 180px;
  vertical-align: top;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
}
.y-header-www .user a {
  padding-left: 10px;
  vertical-align: top;
  color: #fff;
  border-left: 1px solid #fff;
}
.y-header-www .user a:hover {
  color: #f4ad23;
}

/*用户登陆信息*/
.login-info .icons {
  background-image: url(../image/icons/pc-icons2.png?v=2.0.1);
}
/*2018.5.4 e*/
.login-info {
  position: relative;
  float: right;
}
/*已登陆*/
.login-info .user-info {
  position: relative;
  width: 60px;
  height: 60px;
  text-align: center;
}
.login-info .user-info a {
  display: block;
  height: 100%;
}
.login-info:hover .user-info a {
  background-color: #f4ad23;
}
.login-info .user-info .avatar {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -20px;
  margin-top: -20px;
  width: 40px;
  height: 40px;
  border-radius: 50%;
}
.login-info .user-list {
  display: none;
  position: absolute;
  right: 0;
  width: 175px;
  box-shadow: 0 5px 5px rgba(26,26,26,0.3);
  z-index: 99;
}
.is-login:hover .user-list {
  display: block;
}
.login-info .user-list li {
  position: relative;
  padding-left: 55px;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  background-color: #fff;
  cursor: pointer;
}
.login-info .icons {
  position: absolute;
  width: 25px;
  height: 25px;
  left: 15px;
  top: 12px;
}
.login-info .user-list li:hover .icons {
  left: 13px;
}
.login-info .icons.icon-home {
  background-position: 0 -100px;
}
.login-info .icons.icon-message {
  background-position: -25px -100px;
}
.login-info .icons.icon-follow {
  background-position: -50px -100px;
}
.login-info .icons.icon-like {
  background-position: -75px -100px;
}
.login-info .icons.icon-fans {
  background-position: -100px -100px;
}
.login-info .icons.icon-invite {
  background-position: -125px -100px;
}
.login-info .icons.icon-agency {
  background-position: -150px -100px;
}
.login-info .icons.icon-planer {
  background-position: -175px -100px;
}
.login-info .icons.icon-auth {
  background-position: -225px -100px;
}
.login-info .icons.icon-logout {
  background-position: -200px -100px;
}
.login-info .icons.icon-orderlist {
  background-position: -519px -100px;
}
.login-info .user-list li:hover {
  border-left: 2px solid #f4ad23;
  padding-left: 53px;
  background-color: #f4f4f4;
}
.login-info .user-list li a {
  display: block;
  color: #1a1a1a;
}
.login-info .user-list li:hover a {
  color: #f4ad23;
}
.login-info .user-list span {
  position: absolute;
  top: 7px;
  padding: 0 5px;
  height: 15px;
  line-height: 15px;
  -webkit-text-size-adjust:none;
  font-size:9px;
  color: #fff;
  background-color: #ff4a3a;
  border-radius: 8px;
}
/*已登陆 end*/
/*未登录*/
.login-info .txt {
  display: inline-block;
  margin-left: 34px;
  color: #b5b5b5;
  cursor: pointer;
}
.login-info .txt:hover,
.login-info .txt.active {
  color: #f4ad23;
}
/*未登录 end*/
/*用户登陆信息 end*/
/*顶部导航栏 end*/

