/*注册屏*/
.loginmain {
  position: relative;
  width: 100%;
  min-width: 960px;
  margin: 0 auto;
  height: 435px;
  background-image: url(//artzhe.oss-cn-shenzhen.aliyuncs.com/static/mp/mpBanner.jpg?x-oss-process=image/resize,w_1920,q=50,image/format,jpg);
  -webkit-background-size: cover;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
  z-index: 1;
}
.loginpanel {
  position: relative;
  width: 300px;
  height: 435px;
  padding: 0 25px;
  color: #fff;
  background: rgba(0, 0, 0, .2);
  overflow: hidden;
}
.loginpanel:after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .4);
}
.loginpanel .blur {
  position: absolute;
  width: 1920px;
  height: 435px;
  top: 0;
  right: -480px;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  /* blur */
  filter: url(blur-8px.svg#blur);
  /* css3 */
  -webkit-filter: blur(8px);
  -moz-filter: blur(8px);
  filter: blur(8px); /* FireFox 35+ support */
  /* IE6~IE9 */
  filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=8, MakeShadow=false);
  *left: -8px; left: -8px\0;
}
.loginpanel h2 {
  font-size: 24px;
  line-height: 1;
  padding-bottom: 20px;
  color: #fff;
  text-align: center;
}
.loginpanel .errortip {
  margin-top: 80px;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
  color: #e82c3a;
}
.loginpanel .to-login,
.loginpanel .logined {
  position: relative;
  z-index: 1;
}
.loginpanel .login-item {
  position: relative;
  margin-bottom: 9px;
  height: 48px;
}
.loginpanel .login-l {
  position: absolute;
  left: 0;
  top: 0;
  width: 40px;
  height: 40px;
  background-color: transparent;
}
.loginpanel .input {
  padding: 10px 12px 10px 40px;
  height: 40px;
  font-size: 14px;
  line-height: 20px;
  border: none;
  color: #fff;
  background-color: rgba(0,0,0, 0.2);
}
.loginpanel .btn-group {
  padding: 40px 0 40px;
}
.loginpanel .btn {
  display: block;
  width: 100%;
  border-radius: 0;
}
.link {
  font-size: 14px;
  line-height: 1;
  color: #989898;
  cursor: pointer;
}
.loginpanel .icons {
  width: 40px;
  height: 40px;
}
.icons.icon-phone {
  background-position: -440px -300px;
}
.icons.icon-pass {
  background-position: -400px -300px;
}
.loginmain .slogan {
