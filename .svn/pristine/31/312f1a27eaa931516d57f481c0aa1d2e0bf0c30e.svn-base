  <script type="text/javascript">
    $(function() {
        var winS,winH,$directorys,$datas,$pages,$dls,$welfares,$events,$items,sections;
        $items = $(".part");
        winH = $(window).height();

        // 进场动画
        $(".banner_content").children().each( function(index) {
            $(this).css("animation-delay",1.5 + index/3 + "s").addClass("animated fadeIn");
        });
        function addAnimated() {
            winS = $(window).scrollTop();
            // 动画
            $items.each( function(index) {
                var elT = $(this).offset().top;
                if (winS + winH - 100 > elT) {
                    $(this).addClass("animated");
                }
            });
        }
        $(function () {
          addAnimated();
        });
        
        $(window).scroll( function() {
          addAnimated();
        });

    });
