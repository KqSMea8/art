.edit-title{padding:16px 0 16px 20px;border-bottom: 1px solid #e2e2e2; font-size: 22px;color: #666;}
.edit-title span{font-size: 14px;color: #999;}
.edit-l{float: left;width: 250px;padding:20px 10px 10px;padding-bottom: 20000px;margin-bottom: -20000px; background-color: #e9e9e9;}
.edit-ltitle{padding:0 0 10px 10px; font-size: 14px;color: #444;}
.edit-lUl.finished li:first-child .edit-rImgStatu{background-position: -464px -222px; }
.edit-lUl li:first-child .edit-img{background:#ececec url(/image/upload/bgadd.png) center no-repeat; background-size: 100%;}
.edit-wrap{overflow: hidden;}
.edit-r{overflow: hidden;}
.edit-lUl li{margin-bottom: 20px;cursor: pointer;}
.edit-lUl li.cur{border:2px solid #f5b230}
.edit-lUl li.noclick{cursor: auto;}
.edit-img{position: relative;height: 230px;background-position: center;background-size: 100%;background-repeat: no-repeat;background-color: #000;}
.edit-imgTit{position: absolute;bottom:0; left: 0;padding-left: 13px; width: 100%; height: 30px;line-height: 30px; font-size: 14px;color: #fff;background-color: rgba(0,0,0,.4)}
.edit-con{position: relative; padding:6px; background-color:#fff; color: #666}
.edit-contit{height: 21px;font-size: 14px;font-weight: bold;line-height: 21px;text-overflow: ellipsis;overflow: hidden;width: 180px;white-space: nowrap;}
.edit-contit:before{content:'';display: inline-block;width: 5px;margin-right: 6px; height:100%;vertical-align: bottom; background-color: #e9ab32;}
.edit-condate{padding: 6px 0 0 12px; font-size: 14px;}
.edit-c{padding:0 18px;width: 754px;float: left;}
.edit-r{float: right;width: 185px;padding:18px 10px 0;padding-bottom: 20000px;margin-bottom: -20000px; border-left:1px solid #e2e2e2;}
.edit-rTit{padding-bottom: 10px; font-size: 14px;color: #444;}
.edit-rUl li{border:1px solid #e9e9e9;padding-left: 55px; height: 48px;line-height: 48px; color: #444;font-size: 14px;}
.edit-rUl li:hover{border:2px solid #e9ab32;cursor: pointer;}

#j_upload_img_btn{background:url(data:image/png;base64,) 10px center no-repeat;}

