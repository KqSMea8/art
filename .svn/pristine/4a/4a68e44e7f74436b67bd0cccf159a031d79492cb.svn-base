var vmAuth = new Vue({
  el: '#app',
  data: {
    isAgree: true,
    ruleShow: true,
    boxIsShow: false,
  },
  methods: {
    agreeToggle: function () {
      this.isAgree = !this.isAgree;
    },
    goToFirst: function () {
      console.log(this.isAgree);
      if (this.isAgree) {
        console.log('tongyi');
        window.location.href = '/home/auth/first';
      } else {
        this.$message({
          message: '请勾选《艺术者认证服务协议》',
          type: 'warning'
        });
        return false;
      }
    },
    showXieyi: function () {
      this.boxIsShow = true;
    },
    hideBox: function () {
      this.boxIsShow = false;
    },
    agreeRule: function () {
      this.isAgree = true;
      this.boxIsShow = false;
    }
  }
});