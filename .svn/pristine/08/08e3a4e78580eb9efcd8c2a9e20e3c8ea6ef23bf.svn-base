@charset "utf-8";

/*CSS reset*/
html, body { min-height: 100%; }
html { background: #fff; -webkit-text-size-adjust: 100%; }
html * { box-sizing: border-box; -webkit-text-size-adjust: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0); outline: none; }
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, fieldset, lengend, button, input, textarea, th, td, menu { margin: 0; padding: 0; }
body { font: 100%/1.5 'Heiti SC', 'Helvetica', 'HelveticaNeue', 'Droidsansfallback', "Droid Sans", 'Dengxian', 'Segoe', 'microsoft yahei'; color: #191919; }
input, select, textarea, button { font: inherit; font-size: 100% }
table { border-collapse: collapse; border-spacing: 0 }
img, embed, object, video, iframe { width: auto; max-width: 100%; height: auto; }
img { border: 0 }
del { text-decoration: line-through }
address, caption, cite, code, dfn, em, th, var { font-style: normal; font-weight: 500 }
code, kbd, pre, samp, tt { font-family: "Courier New", Courier, monospace; }
ol, ul { list-style: none }
h1, h2, h3, h4, h5, h6 { font-size: 100%; }
a { text-decoration: none; display: block; }
a:link, a:visited { color: #6ea6c3; }
a:hover, a:active { color: #333; }
input[type="button"], input[type="submit"], input[type="reset"], input[type="file"]::-webkit-file-upload-button, button { -webkit-appearance: none; }
i { font-style: normal; }
[v-cloak] {
  display: none;
}

/*CSS通用样式库--可以在任何网站使用的CSS样式库*/
.fix:after, .fix:before { display: block; content: "clear"; height: 0; clear: both; overflow: hidden; visibility: hidden; }
.l, .fl { float: left; }
.r, .fr { float: right; }
.dpb { display: block; }
.dpib { display: inline-block; }
.pr { position: relative; }
.cc { width: 93.6%; margin: 0 auto; }
::-moz-placeholder { color: #cacaca; }
::-webkit-input-placeholder { color: #cacaca; }
:-ms-input-placeholder { color: #cacaca; }

/*基于750px宽度，左右间距为24px,居中显示盒子*/
.flexWrap, .flex { display: -webkit-box; display: -webkit-flex; display: flex; }
.flexItem, .flexi { text-align: center; -webkit-box-flex: 1; -webkit-flex: 1; flex: 1; }
.tableA { display: table; }
.tableCell { display: table-cell; 
vertical-align: middle; }
.lh { line-height: 1; }
.tdu { text-decoration: underline; }
.vam { 
vertical-align: middle; }
.wsn { white-space: nowrap; }

/*公用部分*/
.break { word-break: break-all; word-wrap: break-word }

/*font-weight*/
.fwb { font-weight: bold; }
.fwn { font-weight: normal; }
.fwl { font-weight: lighter; }
.ellipsis { white-space: nowrap; text-overflow: ellipsis; overflow: hidden; }

/*网站CSS样式库--根据当前实际的项目内容指定的*/

/*背景颜色*/ 
html, body { background-color: #efeff4; }
body { max-width: 10rem; margin: 0 auto; width: 100%; overflow-x: hidden; }

/*按钮*/
.btn { display: block; width: 100%; height: 1.01333rem; line-height: 1.01333rem; font-size: 0.4rem; text-align: center; background: #eaab32; color: #333; border: none; border-radius: 0.10667rem; box-shadow: 0.08rem 0.08rem 0.08rem rgba(61, 46, 14, .24); }
.btn2 { display: block; width: 100%; height: 1.01333rem; line-height: 1.01333rem; font-size: 0.4rem; text-align: center; background: #eaab32; color: #333; border: none; border-radius: 0.10667rem; }

/*弹框提示*/
.tipshow { min-width: 5.0rem; min-height: 1.5625rem; position: fixed; left: 50%; top: 50%; margin-left: -2.5rem; margin-top: -0.78125rem; -webkit-border-radius: 0.0625rem; border-radius: 0.0625rem; z-index: 100000; }
.tipshow .tipbg { background-color: rgba(0, 0, 0, 0.8); width: 100%; height: 100%; position: absolute; left: 0; top: 0; z-index: 0; -webkit-border-radius: 0.0625rem; border-radius: 0.0625rem; }
.tipshow .fontwrap { display: table; width: 100%; min-height: 1.5625rem; height: 1.5625rem; position: absolute; left: 0; top: 0; z-index: 1; }
.tipshow .font { color: #fff; font-size: 0.375rem; line-height: 0.53125rem; position: relative; text-align: center; display: table-cell; 
vertical-align: middle; padding: 0px 0.46875rem; }
.tipshow.effect { -webkit-animation: fadeIn 1s 0s ease both; -moz-animation: fadeIn 1s 0s ease both; }
@-webkit-keyframes fadeIn {
  0% { opacity: 0 }
  100% { opacity: 1 }
}
@-moz-keyframes fadeIn {
  0% { opacity: 0 }
  100% { opacity: 1 }
}

/*/弹框提示*/

/*头像*/
.avatar { border-radius: 50%; }

/*公用部分 end*/

/* CSS3动画 */
.animated { -webkit-animation-duration: .8s; animation-duration: .8s; -webkit-animation-fill-mode: both; animation-fill-mode: both }

/*fadeInUp*/
@keyframes fadeInUp {
  0% { opacity: 0; -webkit-transform: translate3d(0, 100%, 0); -ms-transform: translate3d(0, 100%, 0); transform: translate3d(0, 100%, 0) }
  100% { opacity: 1; -webkit-transform: translate3d(0, 0, 0); -ms-transform: translate3d(0, 0, 0); transform: translate3d(0, 0, 0); }
}
.fadeInUp { -webkit-animation-name: fadeInUp; -moz-animation-name: fadeInUp; -ms-animation-name: fadeInUp; animation-name: fadeInUp }

/*fadeInUp end*/

@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }

  to {
    opacity: 0;
  }
}

@keyframes fadeOut {
  from {
    opacity: 1;
  }

  to {
    opacity: 0;
  }
}

.fadeOut {
  -webkit-animation-name: fadeOut;
  animation-name: fadeOut;
}

@keyframes fadeInAndOut {
  0% { opacity: 0;}
  25% { opacity: 1; }
  75% { opacity: 1; }
  100% { opacity: 0;}
}
@-webkit-keyframes fadeInAndOut {
  0% { opacity: 0; display: none;}
  25% { opacity: 1; display: block;}
  75% { opacity: 1; display: block;}
  100% { opacity: 0; display: none;}
}
.fadeInAndOut {
  -webkit-animation-name: fadeInAndOut;
  animation-name: fadeInAndOut;
}

/* 弹出动画 */
@-webkit-keyframes anim-scale { /* 默认 */
    0% {opacity: 0; -webkit-transform: scale(.5); transform: scale(.5)}
    100% {opacity: 1; -webkit-transform: scale(1); transform: scale(1)}
}
@keyframes anim-scale { /* 默认 */
    0% {opacity: 0; -webkit-transform: scale(.5); transform: scale(.5)}
    100% {opacity: 1; -webkit-transform: scale(1); transform: scale(1)}
}
.anim-scale{animation-name:  anim-scale; -webkit-animation-name:  anim-scale;}

@-webkit-keyframes anim-up{
  0%{opacity: 0; -webkit-transform: translateY(25.0rem); transform: translateY(25.0rem)}
  100%{opacity: 1; -webkit-transform: translateY(0); transform: translateY(0)}
}
@keyframes anim-up{
  0%{opacity: 0; -webkit-transform: translateY(25.0rem); transform: translateY(25.0rem)}
  100%{opacity: 1; -webkit-transform: translateY(0); transform: translateY(0)}
}
.anim-up{-webkit-animation-name: anim-up;animation-name: anim-up}

/* 弹出动画 end */

