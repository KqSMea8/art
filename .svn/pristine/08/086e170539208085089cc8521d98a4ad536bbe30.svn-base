
/*vue数据未加载完成时隐藏*/
[v-cloak] {
  display: none !important;
}


.w-350 {
  width: 350px;
}
.w-435 {
  width: 435px;
}
/* 弹出动画 */
@-webkit-keyframes anim-scale { /* 默认 */
    0% {opacity: 0; -webkit-transform: scale(.5); transform: scale(.5)}
    100% {opacity: 1; -webkit-transform: scale(1); transform: scale(1)}
}
@keyframes anim-scale { /* 默认 */
    0% {opacity: 0; -webkit-transform: scale(.5); transform: scale(.5)}
    100% {opacity: 1; -webkit-transform: scale(1); transform: scale(1)}
}
.anim-scale{animation-name:  anim-scale; -webkit-animation-name:  anim-scale;}



/* 弹出层 */
.layerbox {
  /*display: none;*/
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
}
.layershade {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0, .6);
  pointer-events: auto;
}
.thirdLayerIn {
  position: fixed;
  /*display: none;*/
  top: 50%;
  left: 50%;
  background-color: #fff;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation-duration: .2s;
  animation-duration: .2s;
}

/*弹出层 end*/

/*协议*/
#xieyi{
	width:900px;
	border:solid 10px #b3b3b3;
	background-color:#ffffff;
	height:500px;
	position:fixed;
	top:50%;
	left:50%;
	margin-top:-250px;
	margin-left:-450px;
	overflow:hidden;
	z-index:9999;
	/*display:none;*/
}
#xieyi h1{
	border-bottom:solid 1px #cccccc;
	background-color:#eeeeee;
	height:39px;
	line-height:39px;
	text-align:center;
	font-size:18px;
	font-family:"microsoft yahei";
}
#xieyi h3 {
  font-weight: 700;
}
#xieyi h1 em{
	float:right;
	display:inline-block;
	width:20px;height:20px;
	cursor:pointer;
	margin:0px 10px 0 0px;
}
#xieyi .btn_agreen{
	padding-top:26px;
	height:100px;
	background-color:#f8f8f8;
	text-align:center;

}
#xieyi .btn_agreen .btnRegister{
	padding: 0 24px;
  font-size: 16px;
  line-height: 48px;
  color: #141414;
  background-color: #f4ad23;
  border-radius: 2px;
  cursor: pointer;
}
#xieyi .content{
	text-align:left;
	height:345px;
	background-color:#ffffff;
	width:880px;
	overflow:auto;
	padding:30px 50px 0 50px;
	line-height:1.5;
	font-size: 14px;
}
/*协议 end*/

