/*覆盖下拉多选*/
.el-select-dropdown.is-multiple .el-select-dropdown__item{border:1px solid transparent; color: #999;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n){background-color: #faf2d9;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n+1){background-color: #e0fbe2;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n+2){background-color: #e0fbf9;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n+3){background-color: #fce8f3;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n+4){background-color: #dfe4ff;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item:nth-child(6n+5){background-color: #fbe5e0;}
.el-select-dropdown.is-multiple li.el-select-dropdown__item.selected{background-color: #e7e7e7;color: #999;}
.el-select-dropdown.is-multiple .el-select-dropdown__item.hover{color:#f4ad23;}
.el-select-dropdown.is-multiple .el-select-dropdown__item.selected.hover{border:1px solid transparent;color: #999;background-color:#e7e7e7 }
 .el-select-dropdown.is-multiple .el-select-dropdown__item.selected::after{display: none;}
  .el-select__tags > span .el-tag{color: #999}
 .el-select__tags span .el-tag:nth-child(n){background-color: #faf2d9;}
 .el-select__tags span .el-tag:nth-child(2n){background-color: #e0fbe2;}
 .el-select__tags span .el-tag:nth-child(3n){background-color: #e0fbf9;}
 .el-select__tags span .el-tag:nth-child(2n+2){background-color: #fce8f3;}
 .el-select__tags span .el-tag:nth-child(2n+3){background-color: #dfe4ff;}
 .el-tag--primary{border:none;}

