html, body {
  height: 100%;
}
body {
  background-color: #eee;
}
span {
  display: inline-block;
  vertical-align: middle;
}
.artist-content a {
  color: #333;
}
#main {
  margin-bottom: 1.33333rem;
}
.icons.icon-artist {
  width: 0.48rem;
  height: 0.48rem;
  background-position: -0.29333rem -5.26667rem;
}
a {
  vertical-align: middle;
}

/*个人信息*/
.userinfo {
  position: relative;
  color: #666;
  background-color: #fff;
  box-shadow: 0 0.08rem 0.10667rem rgba(0,0,0,.15);
}
.userinfo a {
  color: #666;
}
.userinfo .avatar {
  position: absolute;
  top: 0.24rem;
  left: 0.33333rem;
  width: 1.6rem;
  height: 1.6rem;
  box-shadow: 0 0.08rem 0.13333rem rgba(0,0,0, .3);
}
.userinfo .detail,
.userinfo .invite {
  position: relative;
  padding-left: 2.26667rem;
  line-height: 1.5;
}
.userinfo .detail {
  padding-top: 0.4rem;
  padding-bottom: 0.2rem;
}
.userinfo .desc {
  display: block;
  font-size: 0.4rem;
}
.userinfo .nickname {
  display: inline-block;
  font-size: 0.48rem;
  font-weight: 700;
  vertical-align: middle;
  color: #333;
}
.userinfo .artist,
.userinfo .artist span {
  display: inline-block;
  vertical-align: middle;
}
.icons.icon-set {
  position: absolute;
  right: 0.42rem;
  top: 0.46667rem;
  width: 1.0rem;
  height: 1.0rem;
  background-position: -3.0rem -5.0rem;
}
.userinfo .invite {
  padding-bottom: 0.26667rem;
  padding-right: 0.6rem;
}

.userinfo .invite span {
  display: inline-block;
  vertical-align: middle;
  font-size: 0.32rem;
}
.icons.icon-question {
  width: 0.48rem;
  height: 0.48rem;
  background-position: -1.28rem -5.26667rem;
}
.icons.icon-arrow {
  float: right;
  width: 0.48rem;
  height: 0.48rem;
  background-position: -2.33333rem -5.26667rem;
}
/*个人信息*/
.appeal {
  width: 9.36rem;
  margin: 0 auto;
  padding: 0 1.93333rem;
  height: 0.8rem;
  border-bottom: 2px solid #e6e6e6;
}
.appeal .btn-appeal {
  display: inline-block;
  font-size: 0.32rem;
  line-height: 1;
  padding: 0.13333rem;
  color: #333;
  background-color: #eee;
  border-radius: 4px;
}

/* my-bar-list */
.my-bar-list {
  position: relative;
  table-layout: fixed;
  width: 100%;
  padding-top: 0.16rem;
}
.my-bar-item {
  position: relative;
  display: table-cell;
  width: 1%;
  text-align: center;
  vertical-align: middle;
}
.my-bar-item .icons {
  position: relative;
  margin: 0 auto;
  width: 0.6rem;
  height: 0.6rem;
}
.my-bar-item .icons,
.my-bar-label {
  display: block;
  position: relative;
}
.my-bar-item .my-bar-label {
  height: 0.64rem;
  line-height: 0.64rem;
  font-size: 0.32rem;
  color: #666;
}
/*.my-bar-item.on .my-bar-label {
  color: #edb22f;
}*/
.my-bar-item .icons.icon-follow {
  background-position: -9.2rem -0.1rem;
}

.my-bar-item .icons.icon-like {
  background-position: -7.18rem -0.1rem;
}
.my-bar-item .icons.icon-msg {
  background-position: -6.18667rem -0.1rem;
}

/* my-bar-list end */

/*欣赏者详情*/
.go-enjoy {
  position: relative;
  margin: 4.46667rem 0.32rem 0;

}
.go-enjoy .btn-go-enjoy {
  height: 1.17333rem;
  line-height: 1.17333rem;
  font-size: 0.48rem;
  font-weight: 700;
  color: #333;
  box-shadow: none;
}
.go-enjoy .go-enjoy-info {
  position: absolute;
  top: -2.66667rem;
  left: 50%;
  margin-left: -2.93333rem;
  padding: 0.24rem 0.32rem 0.4rem;
  font-size: 0.4rem;
  line-height: 1.5;
  text-align: center;
  background-color: #efc87d;
  border-radius: 0.10667rem;
}
.go-enjoy-info::after {
  content: '';
  position: absolute;
  bottom: -0.26667rem;
  left: 0.77333rem;
  margin-left: -0.12rem;
  width: 0;
  height: 0;
  border-top: 0.26667rem solid #efc87d;
  border-left: 0.17333rem solid transparent;
  border-right: 0.17333rem solid transparent;
}

  /*欣赏着动态*/
.trend {
