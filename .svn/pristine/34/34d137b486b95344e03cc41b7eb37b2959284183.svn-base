/* 发布新作品按钮 */
#add {
  position: absolute;
  bottom: 2.53333rem;
  right: 0.53333rem;
}
#add .btn-add {
  width: 1.33333rem;
  height: 1.33333rem;
  background-position: 0 -9.0rem;
}
#add .publish {
  position: absolute;
  right: 1.66rem;
  top: -0.6rem;
  padding: 0.16rem 0.4rem 0.16rem 0;
  width: 5.33333rem;
  font-size: 0.4rem;
  text-align: right;
  line-height: 0.64rem;
  border-radius: 0.13333rem;
  background-color: #efc87d;
  -webkit-animation-duration: 3.5s;
  animation-duration: 3.5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
#add .publish::after {
  content: '';
  position: absolute;
  bottom: 0.33333rem;
  right: -0.2rem;
  width: 0;
  height: 0;
  border-top: 0.16rem solid transparent;
  border-left: 0.21333rem solid #efc87d;
  border-bottom: 0.16rem solid transparent;
}
/* 发布新作品按钮 end */
.upload {
  position: absolute;
  opacity: 0;
  bottom: 1.41333rem;
  right: 0.48rem;
  padding: 0 0.32rem;
  font-size: 0.32rem;
  line-height: 0.72rem;
  background-color: #efc87d;
  border-radius: 4px;
  animation-delay: 4s;
  -webkit-animation-duration: 3.5s;
  animation-duration: 3.5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
.upload::after {
  content: '';
  position: fixed;
  bottom: 1.30667rem;
  left: 50%;
  margin-left: -0.12rem;
  width: 0;
  height: 0;
  border-top: 0.13333rem solid #efc87d;
  border-left: 0.12rem solid transparent;
  border-right: 0.12rem solid transparent;
}


/* 艺术家详情 end */


