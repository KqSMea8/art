@charset "utf-8";
/* CSS Document */

#artzhePop_iframe{
	border:0px;
	}
#artzhe_pop_container {
	z-index:9999;
   
    width: 100%;
    height: auto;
    min-height: 550px;
    position: fixed;
    background-color:2c2c2c;
    background-color:rgba(0,0,0,0.6);
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
   
}



#artzhe_pop_box {
	
    width: 880px;
    height: 460px;
    position: absolute;
    background-color: #fff;
    top: 8%;
    left: 8%;
    margin: 0 0 0 0px;
    z-index: 100;
    opacity: 1;   

    /*border: 5px solid  #666;*/
}

#artzhe_pop_box_close {
    width: 32px;
    height: 32px;
    position: absolute;
    top: -30px;
    right: -30px;
    cursor: pointer;
    background-image: url("icon_close.png");
    z-index: 999;
}

