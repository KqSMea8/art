.view {
  /* font-size: 0.42667rem; */
  font-size: .37rem;
  line-height: 1.75;
}
.view * {
  margin: 0;
  padding: 0
}
.view a {
  color: #607fa6;
  text-decoration: none
}
.view {
  word-wrap: break-word;
  word-break: break-all
}
.view .rich_media_thumb_wrp {
  margin-bottom: 0.16rem;
}
.view .rich_media_thumb_wrp .original_img_wrp {
  display: block
}
.view .rich_media_thumb {
  display: block;
  width: 100%
}
.view {
  overflow: hidden;
  color: #3e3e3e
}
.view img {
  height: auto !important
}
.view img[data-ratio] {
  height: auto !important
}
.view * {
  max-width: 100% !important;
  box-sizing: border-box !important;
  -webkit-box-sizing: border-box !important;
  word-wrap: break-word !important;
  word-break: normal !important;
  word-wrap: break-word;
  -webkit-hyphens: auto;
  -ms-hyphens: auto;
  hyphens: auto
}
.wrap img {
  margin: 0.24rem auto 0;
  display: block;
  width: 100%;
  height: auto;
}
.wrap video {
  margin: 0 auto;
  display: block;
  width: 100%;
  height: auto;
  margin-top: 0.24rem;
}
.view p {
  clear: both;
  min-height: 1em;
  white-space: pre-wrap
}
.view em {
  font-style: italic
}
.view fieldset {
  min-width: 0
}
.view .list-paddingleft-2 {
  padding-left: 0.4rem;
}
.view blockquote {
  margin: 0;
  padding-left: 0.24rem;
  border-left: 3px solid #dbdbdb
}
.view .original_img_wrp {
  display: inline-block;
  font-size: 0
}
.view .original_img_wrp .tips_global {
  display: block;
  margin-top: .5em;
  font-size: 0.4rem;
  text-align: right;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal
}
.view .rich_media_tool {
  overflow: hidden;
  padding-top: 0.4rem;
  line-height: 0.85333rem;
}
.view .rich_media_tool .meta_primary {
  float: left;
  margin-right: 0.24rem
}
.view .rich_media_tool .meta_extra {
  float: right;
  margin-left: 0.24rem
}
.view .res_iframe {
  display: block;
  width: 100%;
  background-color: transparent;
  border: 0
}
.view .vote_area {
  display: block
}
.view .qqmusic_iframe {
  width: 100% !important;
  height: 2.0rem
}
.view .audio_iframe {
  width: 100% !important;
  height: 2.16rem;
}
.view .video_iframe {
  height: 5.2rem;
  background-color: #000;
  width: 100% !important;
  *width: 96% !important;
  position: static
}
.view .video_small_iframe {
  height: auto !important
}
.view .shopcard_iframe {
  margin: 0.4rem 0;
  height: 2.53333rem
}
.view .topic_iframe {
  width: 100% !important;
  height: 118px;
  margin: 14px 0
}
.view .cpc_iframe {
  height: 210px;
  margin: 14px 0
}
.view .selectTdClass {
  background-color: #edf5fa !important
}
.view table.noBorderTable td, .view table.noBorderTable th, .view table.noBorderTable caption {
  border: 1px dashed #ddd !important
}
.view table {
  margin-bottom: 0.24rem;
  border-collapse: collapse;
  display: table;
  width: 100% !important
}
.view td, .view th {
  word-wrap: break-word;
  word-break: break-all;
  padding: 0.12rem 0.24rem;
  border: 1px solid #DDD
}
.view caption {
  border: 1px dashed #DDD;
  border-bottom: 0;
  padding: 0.08rem;
  text-align: center
}
.view th {
  border-top: 2px solid #BBB;
  background: #f7f7f7
}
.view .ue-table-interlace-color-single {
  background-color: #fcfcfc
}
.view .ue-table-interlace-color-double {
  background-color: #f7faff
}
.view td p {
  margin: 0;
  padding: 0
}

/*插入样式*/
.view .search-user,
.view .search-update {
  display: block;
  width: 100%;
  height: 1.866667rem;
  background: #fff;
  border: 1px solid #e5e5e5;
}
.search-user .user-avatar {
  float: left;
  width: 1.84rem;
  height: 1.84rem;
  background-size: 100% 100%;
}
.search-user .search-user-info {
  float: left;
  padding-left: 0.266667rem;
}
.search-user .user-name {
  font-size: 0.426667rem;
  line-height: 0.96rem;
  color: #333;
}
.search-user .user-type {
  font-size: 0.373333rem;
  line-height: 0.533333rem;
  color: #999;
}
.search-user .user-name,
.search-user .user-type {
  width: 6.666667rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.search-update {
  padding-left: 0.32rem;
}
.search-update .search-title {
  font-size: 0.426667rem;
  line-height: 0.96rem;
  color: #333;
}
.search-update .search-detail {
  font-size: 0.373333rem;
  line-height: 0.533333rem;
  color: #999;
}
.search-update .search-title,
.search-update .search-detail {
  width: 9.0rem;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.search-link {
  position: relative;
  height: 0.64rem;
}
.search-link a:before {
  content: '';
  position: absolute;
  top: 0.106667rem;
  left: 0.053333rem;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
  width: 0.426667rem;
  height: 0.426667rem;
}
.search-link a {
  position: relative;
  display: block;
  padding-left: 0.533333rem;
  font-size: 0.4rem;
  height: 0.64rem;
  line-height: 0.64rem;
}
/*插入样式 end*/

/*插入音乐的样式*/

.view .audioPlayBox {
  display: block;
  width: 100%;
  height: 1.87rem;
  clear: both;
  background: #fff;
  border: 1px solid #ccc;
  cursor: pointer;
}

.view .audioPlayBox .leftPlay {
  display: block;
  position: relative;
  width: 1.84rem;
  height: 1.84rem;
  float: left;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.view .leftPlay .playimg {
  display: block;
  width: 1.84rem;
  height: 1.84rem;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: 0.96rem 0.96rem;
}

.view .leftPlay .playPause {
  display: block;
  width: 1.84rem;
  height: 1.84rem;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: 0.96rem 0.96rem;
}

.view .audioPlayBox .rightDirection {
  display: block;
  border-left: 1px solid #ccc;
  height: 1.84rem;
  padding-left: 0.32rem;
  float: left;
  font-weight: normal;
}

.view .rightDirection .musicTitle {
  display: block;
  font-size: 0.426667rem;
  line-height: 0.96rem;
  overflow: hidden;
}

.view .rightDirection .musicAuthor {
  display: block;
  color: #989898;
  font-size: 0.37rem;
  overflow: hidden;
}
.view .rightDirection .musicTitle,
.view .rightDirection .musicAuthor {
  width: 6.666667rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}


/*插入音乐的样式end*/

