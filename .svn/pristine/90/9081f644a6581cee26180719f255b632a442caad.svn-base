body {
  background-color: #f4f4f4;
}
.banner {
  width: 100%;
  padding-top: 22px;
  height: 140px;
  text-align: center;
  background-position: center center;
  background-repeat: no-repeat;
  background-color: #000;
  background-image: url(../image/user/banner.jpg);
}
.banner .banner-info h2 {
  margin: 0 auto;
  height: 96px;
  width: 162px;
  text-indent: -4000px;
  background: url(../image/user/banner-logo.png) left center no-repeat;
}

/*主内容区*/
#main .content {
  padding: 20px 0 40px;
  min-height: 570px;
  background-color: #f4f4f4;
}
/*左内容区*/
