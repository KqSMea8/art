  background-position: -519px -100px;
}
.my-info .user-list li.active,
.my-info .user-list li:hover {
  border-left: 2px solid #f4ad23;
  padding-left: 53px;
  background-color: #f4f4f4;
}
.my-info .user-list li a {
  color: #1a1a1a;
}
