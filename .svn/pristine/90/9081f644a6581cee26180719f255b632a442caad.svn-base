}

/*个人信息页面*/
.face-wrap {
  /* padding-top: 36px; */
  padding-bottom: 12px;
  margin: 0 auto;
  text-align: center;
  clear: both;
  overflow: hidden;
}
.face-wrap .img-wrap {
  display: inline-block;
  text-align: left;
  position: relative;
  /* margin: 0 auto; */
  width: 48px;
  height: 48px;
  border-radius: 50%;
  overflow: hidden;
  cursor: pointer;
}
.face-wrap .img-wrap img {
  width: 48px;
  height: 48px;
}
.change {
  display: none;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 48px;
  font-size: 12px;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.6);
}
.face-wrap .img-wrap:hover .change {
  display: block;
}
.face-wrap .nick {
  display: inline-block;
  padding-left: 18px;
  padding-right: 12px;
  /* margin-top: 16px;
  margin-bottom: 28px; */
  font-size: 18px;
  line-height: 36px;
}
.cover {
  position: relative;
  width: 100px;
}
.cover .img-wrap {
  position: relative;
  width: 100px;
  height: 100px;
  overflow: hidden;
  cursor: pointer;
}
.cover .img-wrap img {
  width: 100px;
  height: 100px;
}
.cover .img-wrap:hover .change {
  display: block;
}
.change-cover {
  position: absolute;
  left: 110px;
  bottom: 0;
  width: 50px;
  line-height: 1;
}

#qrcode {
  padding: 2px;
  width: 150px;
  height: 150px;
  border: 1px solid #eee;
}
#qrcode img {
  width: 144px;
  height: 144px;
}
/*个人信息页面 end */

/*message 页面*/
.usermsg {
  position: relative;
  background-color: #fff;
}
.usermsg .nav {
  position: relative;
  table-layout: fixed;
  right: 0;
  left: 0;
  top: 0;
  width: 100%;
  height: 57px;
  border-bottom: 1px solid #f4f4f4;
  background-color: #fff;
  z-index: 10;
}
.usermsg .nav li {
  position: relative;
  display: table-cell;
  /*width: 1%;*/
  width: 191px;
  height: 57px;
  font-size: 16px;
  text-align: center;
  vertical-align: middle;
  color: #989898;
  cursor: pointer;
}
.usermsg .nav li.active {
  font-weight: 700;
  color: #1a1a1a;
}
.usermsg .nav li.active:after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 50%;
  margin-left: -35px;
  width: 70px;
  border-bottom: 2px solid #f4ad23;
}
.usermsg .nav .cont {
  position: relative;
}
.usermsg .nav .num {
}
.usermsg .nav li.active .num {
  display: none;
}
.msg-wrap .list {
  padding: 0 22px;
}
.msg-wrap .item {
  position: relative;
  padding: 20px 0;
  border-bottom: 1px solid #f4f4f4;
}
.msg-wrap .item .info {
  position: relative;
  padding-left: 75px;
}
.msg-wrap .item .avatar {
  position: absolute;
  left: 8px;
  top: 10px;
  width: 48px;
  height: 48px;
  border-radius: 50%;
}
.msg-wrap .item h3 {
  padding-top: 10px;
  font-size: 16px;
  line-height: 26px;
}
.msg-wrap .item .time {
  padding-left: 8px;
  font-size: 12px;
  color: #989898;
}
.msg-wrap .item .detail {
  font-size: 14px;
  line-height: 24px;
}
/*message 页面 end*/

