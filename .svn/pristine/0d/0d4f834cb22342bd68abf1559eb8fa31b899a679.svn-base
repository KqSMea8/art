.scale-btn {
	transform: scale(1 , 1);
	animation:scale-btn 1s linear infinite;
	-webkit-animation:scale-btn 1s linear infinite; /* Safari 和 Chrome */
}
@keyframes scale-btn {
    0% {transform: scale(1 , 1);}
    30% {transform: scale(1.1 , 1.1);}
    60% {transform: scale(0.9 , 0.9);}
    100% {transform: scale(1 , 1);}
}
@-webkit-keyframes scale-btn {
    0% {transform: scale(1 , 1);}
    30% {transform: scale(1.1 , 1.1);}
    60% {transform: scale(0.9 , 0.9);}
    100% {transform: scale(1 , 1);}
}

.anim-scale {
  animation-name: anim-scale;
  -webkit-animation-name: anim-scale;
}
@-webkit-keyframes anim-scale {

  /* 默认 */
  0% {
    opacity: 0;
    -webkit-transform: scale(.5);
    transform: scale(.5)
  }
  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}
@keyframes anim-scale {

  /* 默认 */
  0% {
    opacity: 0;
    -webkit-transform: scale(.5);
    transform: scale(.5)
  }
  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}

