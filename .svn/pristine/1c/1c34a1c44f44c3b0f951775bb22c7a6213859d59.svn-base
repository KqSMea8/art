.smalltrp{
  display: inline-block;
  margin-top: 12px;
  font-weight: 100;
}
.smalltrp p{
  display: inline-block;
  position: relative;
  font-size: 14px;
  color: #aaa;
}

.smalltrp p:nth-child(1){
  padding-left: 20px;
  padding-right: 8px;
}

.smalltrp p:nth-child(1)::before{
  width: 16px;
  height: 16px;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -8px;
  display: block;
  content: '';
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 100% 100%;
}

.smalltrp p:nth-child(2){
  padding-left: 20px;
}

.smalltrp p:nth-child(2)::before{
  width: 16px;
  height: 16px;
