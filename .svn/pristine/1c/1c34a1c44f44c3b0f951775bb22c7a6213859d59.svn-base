/*可以在这里添加你自己的css*/
body.view img{ max-width: 100% !important; }
.view * {
  margin: 0;
  padding: 0;
  font-family: -apple-system-font, 'PingFang SC Medium', 'Heiti SC', 'Helvetica', 'HelveticaNeue', 'Droidsansfallback', "Droid Sans", 'Dengxian', 'Segoe', 'microsoft yahei', sans-serif;
}
.view {
  margin: 4px;
}
.view a {
  color: #607fa6;
  text-decoration: none
}
.view {
  word-wrap: break-word;
  word-break: break-all
}
.view .rich_media_thumb_wrp {
  margin-bottom: 6px;
}
.view .rich_media_thumb_wrp .original_img_wrp {
  display: block
}
.view .rich_media_thumb {
  display: block;
  width: 100%
}
.view {
  overflow: hidden;
  color: #3e3e3e
}
.view img[data-ratio] {
  height: auto !important
}
.view * {
  max-width: 100% !important;
  box-sizing: border-box !important;
  -webkit-box-sizing: border-box !important;
  word-wrap: break-word !important;
  word-break: normal !important;
  word-wrap: break-word;
  -webkit-hyphens: auto;
  -ms-hyphens: auto;
  hyphens: auto
}
.view p {
  clear: both;
  min-height: 1em;
  white-space: pre-wrap
}
.view em {
  font-style: italic
}
.view fieldset {
  min-width: 0
}
.view .list-paddingleft-2 {
  padding-left: 30px;
}
.view blockquote {
  margin: 0;
  padding-left: 9px;
  border-left: 3px solid #dbdbdb
}
.view .original_img_wrp {
  display: inline-block;
  font-size: 0
}
.view .original_img_wrp .tips_global {
  display: block;
  margin-top: .5em;
  font-size: 14px;
  text-align: right;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal
}
.view .rich_media_tool {
  overflow: hidden;
  padding-top: 15px;
  line-height: 32px
}
.view .rich_media_tool .meta_primary {
  float: left;
  margin-right: 10px
}
.view .rich_media_tool .meta_extra {
  float: right;
  margin-left: 10px
}
.view .res_iframe {
  display: block;
  width: 100%;
  background-color: transparent;
  border: 0
}
.view .vote_area {
  display: block
}
.view .qqmusic_iframe {
  width: 100% !important;
  height: 75px
}
.view .audio_iframe {
  width: 100% !important;
  height: 82px
}
.view .video_iframe {
  height: 195px;
  background-color: #000;
  width: 100% !important;
  *width: 96% !important;
  position: static
}
.view .video_small_iframe {
  height: auto !important
}
.view .shopcard_iframe {
  margin: 14px 0;
  height: 95px
}
.view .topic_iframe {
  width: 100% !important;
  height: 118px;
  margin: 14px 0
}
.view .topic_iframe {
  width: 100% !important;
  height: 118px;
  margin: 14px 0
}
.view .weapp_app_iframe {
  height: 330px;
  margin: 14px 0
}
.view .cpc_iframe {
  height: 210px;
  margin: 14px 0
}
.view .selectTdClass {
  background-color: #edf5fa !important
}
.view table.noBorderTable td, .view table.noBorderTable th, .view table.noBorderTable caption {
  border: 1px dashed #ddd !important
}
.view table {
  margin-bottom: 10px;
  border-collapse: collapse;
  display: table;
  width: 100% !important
}
.view td, .view th {
  word-wrap: break-word;
  word-break: break-all;
  padding: 5px 10px;
  border: 1px solid #DDD
}
.view caption {
  border: 1px dashed #DDD;
  border-bottom: 0;
  padding: 3px;
  text-align: center
}
.view th {
  border-top: 2px solid #BBB;
  background: #f7f7f7
}
.view .ue-table-interlace-color-single {
  background-color: #fcfcfc
}
.view .ue-table-interlace-color-double {
  background-color: #f7faff
}
.view td p {
  margin: 0;
  padding: 0
}



/*插入样式*/
.view .search-user,
.view .search-update {
  display: block;
  width: 460px;
  height: 70px;
  background: #fff;
  border: 1px solid #e5e5e5;
}
.search-user .user-avatar {
  float: left;
  width: 68px;
  height: 68px;
  background-size: 100% 100%;
}
.search-user .search-user-info {
  float: left;
  padding-left: 10px;
}
.search-user .user-name {
  font-size: 16px;
  line-height: 36px;
  color: #333;
}
.search-user .user-type {
  font-size: 14px;
  line-height: 20px;
  color: #999;
}
.search-update {
  padding-left: 12px;
}
.search-update .search-title {
  font-size: 16px;
  line-height: 36px;
  color: #333;
}
.search-update .search-detail {
  font-size: 14px;
  line-height: 20px;
  color: #999;
}
.search-update .search-title,
.search-update .search-detail {
  width: 440px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.search-link {
  position: relative;
  height: 24px;
}
.search-link a:before {
  content: '';
  position: absolute;
  top: 4px;
  left: 2px;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
  width: 16px;
  height: 16px;
}
.search-link a {
  position: relative;
  display: block;
  padding-left: 20px;
  height: 24px;
  line-height: 24px;
}
/*插入样式 end*/

/*插入音频的样式*/

.view .audioPlayBox {
  display: block;
  width: 460px;
  height: 70px;
  clear: both;
  background: #fff;
  border: 1px solid #ccc;
  cursor: pointer;
}

.view .audioPlayBox .leftPlay {
  display: block;
  position: relative;
  width: 70px;
  height: 68px;
  float: left;
}

.view .leftPlay .playimg {
  display: block;
  width: 69px;
  height: 68px;
  background: url("data:image/png;base64,") no-repeat center center;
}

.view .audioPlayBox .rightDirection {
  display: block;
  border-left: 1px solid #ccc;
  height: 69px;
  padding-left: 12px;
  float: left;
  font-weight: normal;
}

.view .rightDirection .musicTitle {
  display: block;
  font-size: 16px;
  margin-top: 12px;
  margin-bottom: 8px;
  max-width: 400px;
  overflow: hidden;
}

.view .rightDirection .musicAuthor {
  display: block;
  color: #989898;
  font-size: 14px;
  max-width: 400px;
  overflow: hidden;
}

.view .rightDirection .musicTitle,
.view .rightDirection .musicAuthor {
  width: 370px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}


/*插入音频的样式end*/

