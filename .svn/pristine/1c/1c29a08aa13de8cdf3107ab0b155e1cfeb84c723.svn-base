  `id` int(11) NOT NULL AUTO_INCREMENT,
  `parent_id` int(11) NOT NULL DEFAULT '-1' COMMENT '-1表示没有父级其它表示az_comment.id',
  `commenter` int(11) NOT NULL DEFAULT '-1' COMMENT '评论者az_user.id',
  `comment_to` int(11) NOT NULL DEFAULT '-1' COMMENT 'az_user.id',
  `content` text NOT NULL COMMENT '评论内容',
  `create_time` int(11) NOT NULL DEFAULT '0' COMMENT '评论创建时间',
  `topic_id` int(11) NOT NULL DEFAULT '-1' COMMENT '-1未指定type为1az_artwork.id 2则为az_artwork_update_log.id',
  `type` tinyint(4) NOT NULL DEFAULT '-1' COMMENT '-1未指定1作品评论2作品更新评论',
  `delete_time` int(11) NOT NULL DEFAULT '0' COMMENT '删除时间',
  `level` tinyint(3) NOT NULL DEFAULT '1' COMMENT '1为第一层评论2为评论下面的回复',
  `is_published` enum('N','Y') NOT NULL DEFAULT 'Y' COMMENT '是否立即发布 主要是为了后台假数据评论准备的',
  `publish_time` int(11) NOT NULL DEFAULT '0' COMMENT '发布时间',
  `is_deleted` enum('N','Y') NOT NULL DEFAULT 'N',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='评论';

