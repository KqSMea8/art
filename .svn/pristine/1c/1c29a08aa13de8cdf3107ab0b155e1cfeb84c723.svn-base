-- Table structure for az_user
-- ----------------------------
DROP TABLE IF EXISTS `az_user`;
CREATE TABLE `az_user` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `cn_name` varchar(255) NOT NULL DEFAULT '' COMMENT '中文名',
  `en_name` varchar(255) NOT NULL DEFAULT '' COMMENT '英文名',
  `nickname` varchar(255) NOT NULL,
  `account` varchar(255) NOT NULL DEFAULT '' COMMENT '账户名称',
  `enc_password` varchar(255) NOT NULL DEFAULT '' COMMENT '密文的密码',
  `enc_salt` char(10) NOT NULL DEFAULT '' COMMENT 'salt',
  `password_reset_token` varchar(255) NOT NULL DEFAULT '' COMMENT '密码重置token',
  `birthday` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '生日非unix时间戳',
  `age` double NOT NULL DEFAULT '0',
  `gender` tinyint(3) NOT NULL DEFAULT '3' COMMENT '1男2女3未知',
  `face` int(11) NOT NULL DEFAULT '-1' COMMENT '用户头像-1未指定 az_assets.id',
  `email` varchar(255) NOT NULL DEFAULT '' COMMENT '电子邮件',
  `qq` varchar(255) NOT NULL DEFAULT '',
  `wechat` varchar(255) NOT NULL DEFAULT '',
  `mobile` varchar(255) NOT NULL DEFAULT '',
  `telephone` varchar(255) NOT NULL DEFAULT '',
  `level` tinyint(4) NOT NULL DEFAULT '-1' COMMENT '等级',
  `ip` int(11) NOT NULL DEFAULT '0' COMMENT '用户ipv4',
  `longitude` int(11) NOT NULL DEFAULT '0' COMMENT '精确到小数点后6位的整型值',
  `latitude` int(11) NOT NULL DEFAULT '0' COMMENT '精确到小数点后6位的整型值',
  `mp` bigint(20) NOT NULL DEFAULT '0' COMMENT '用户总积分member point',
  `is_deleted` enum('N','Y') NOT NULL DEFAULT 'N' COMMENT '是否删除',
  `login_times` int(11) NOT NULL DEFAULT '0' COMMENT '登录次数',
  `banned_to` int(11) NOT NULL DEFAULT '-1' COMMENT ' 禁用时间到0永久封禁-1表示正常',
  `from` varchar(100) NOT NULL DEFAULT '' COMMENT '用户来源',
  `invite_code` varchar(255) NOT NULL DEFAULT '' COMMENT '用户邀请码',
  `type` int(11) NOT NULL DEFAULT '-1' COMMENT '用户类型-1未指定0x1欣赏属性0x10艺术家属性0x100收藏者属性',
  `device_info_json` text NOT NULL COMMENT ' 用户设备信息json格式',
  `last_login_time` int(11) NOT NULL DEFAULT '0' COMMENT '上次登录时间',
  `update_time` int(11) NOT NULL DEFAULT '0' COMMENT '资料更新时间',
  `create_time` int(11) NOT NULL DEFAULT '0' COMMENT '账号创建时间',
  `follower_total` int(11) NOT NULL DEFAULT '0' COMMENT ' 关注者总人数',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='基本用户表';

