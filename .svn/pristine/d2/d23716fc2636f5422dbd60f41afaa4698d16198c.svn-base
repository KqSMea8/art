<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="艺术者">
    <meta name="description" content="艺术者">
    <title>艺术家免费认证</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon"/>
    <!-- <link href="//cdn.bootcss.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet" /> -->
    <link href="/Public/Home/js/lib/element/index.css" rel="stylesheet">
    <link href="/Public/Home/css/global.css" rel="stylesheet">
    <link href="/Public/Home/css/gcommon.css" rel="stylesheet">
    <link href="/Public/Home/css/auth.css?v=0.0.1" rel="stylesheet">
  </head>
  <body>
    <div v-cloak id="app">
      <header class="ysz-header">
        <div class="y-header">
          <div class="w clearfix">
            <h1 class="y-head">
            <img class="logo" src="/Public/Home/image/logo.png" alt="logo">
            <span class="y-title">创作平台</span>
            </h1>
          </div>
        </div>
      </header>
      <div id="main">
        <div class="w clearfix">
          <div class="main-wrap mb-72" id="auth">
            <h2 class="title">艺术家免费认证</h2>
            <div class="stage-wrap">
              <p class="info"><i class="icons icon-info"></i>以下信息必须真实可信</p>
              <div class="second-wrap form">
                  <el-form :model="ruleForm" :rules="rules" ref="ruleForm" label-width="457px" class="demo-ruleForm">
                    <el-form-item label="真实姓名" prop="name">
                      <el-input class="w-350" v-model="ruleForm.name" placeholder="请填写您的真实姓名"></el-input>
                    </el-form-item>
                    <el-form-item label="身份证号码" prop="idCardNo">
                      <el-input class="w-350" v-model="ruleForm.idCardNo" placeholder="请填写您的身份证号码" :maxlength="18"></el-input>
                    </el-form-item>
                    <el-form-item label="个性签名" prop="motto">
                      <el-input class="w-435" v-model="ruleForm.motto" placeholder="个人个性签名"></el-input>
                    </el-form-item>
                    <el-form-item label="个人介绍" prop="resume">
                      <el-input class="w-435" type="textarea" :rows="10" v-model="ruleForm.resume" placeholder="个人介绍以及近5年内您的获奖，展览，以及相关荣誉情况"></el-input>
                    </el-form-item>
                    <el-form-item label="个人备注" prop="memo">
                      <el-input class="w-435" type="textarea" :rows="4" v-model="ruleForm.memo" placeholder="填写个人备注信息，提高审核通过率~如个人微信公共号，微博，博客等信息"></el-input>
                    </el-form-item>
                    <div class="btn-group next">
                      <p class="error-text">{{errorTip}}</p>
                      <el-button class="btn-24" type="primary" @click="submitForm('ruleForm')">下一步</el-button>
                    </div>
                  </el-form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <footer>
        <div class="y-footer">
          <div class="w clearfix">
            <p class="mail">联系邮箱：yishuzhe@gaosouyi.com</p>
            <p class="qrcode">
              <img src="/Public/Home/image/qrcode-wx.png">
              <img src="/Public/Home/image/qrcode-app.png">
            </p>
            <p>
              Copyright 2017 www.artzhe.com. All Rights Reserved<br>
              粤ICP备17041531号-1
            </p>
          </div>
        </div>
      </footer>
    </div>
  </body>
  <script src="/Public/Home/js/lib/vue.js"></script>
  <script src="/Public/Home/js/lib/element/index.js"></script>
  <script src="//cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script>
  <script src="http://cdn.bootcss.com/blueimp-md5/1.1.0/js/md5.min.js"></script>
  <script src="/Public/Home/js/service/agent.js"></script>
  <script src="/Public/Home/js/common.js"></script>
  <script src="/Public/Home/js/auth/second.js"></script>
</html>