@charset "utf-8";

/*CSS reset*/
html, body {
  min-height: 100%;
}
html {
  background: #fff;
  -webkit-text-size-adjust: 100%;
}
html * {
  box-sizing: border-box;
  -webkit-text-size-adjust: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  outline: none;
  word-break:break-all;
}
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, fieldset, lengend, button, input, textarea, th, td, menu {
  margin: 0;
  padding: 0;
}
body {
  font: 100%/1.5 'PingFang SC Bold', 'PingFang SC Medium','Heiti SC', 'Microsoft YaHei', 'Helvetica', 'HelveticaNeue', 'Droidsansfallback', "Droid Sans", 'Dengxian', 'Segoe', sans-serif;
  color: #191919;
}
input, select, textarea, button {
  font: inherit;
  font-size: 100%
}
table {
  border-collapse: collapse;
  border-spacing: 0
}
img, embed, object, video, iframe {
  width: auto;
  max-width: 100%;
  height: auto;
}
img {
  border: 0
}
del {
  text-decoration: line-through
}
address, caption, cite, code, dfn, em, th, var {
  font-style: normal;
  font-weight: 500
}
code, kbd, pre, samp, tt {
  font-family: "Courier New", Courier, monospace;
}
ol, ul {
  list-style: none
}
h1, h2, h3, h4, h5, h6 {
  font-size: 100%;
}
a {
  text-decoration: none;
  display: block;
}
a:link, a:visited {
  color: #6ea6c3;
}
a:hover, a:active {
  color: #6ea6c3;
}
input[type="button"], input[type="submit"], input[type="reset"], input[type="file"]::-webkit-file-upload-button, button {
  -webkit-appearance: none;
}
i {
  font-style: normal;
}
[v-cloak] {
  display: none !important;
}

/*CSS通用样式库--可以在任何网站使用的CSS样式库*/
.fix:after, .fix:before {
  display: block;
  content: "clear";
  height: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}
.l, .fl {
  float: left;
}
.r, .fr {
  float: right;
}
.dpb {
  display: block;
}
.dpib {
  display: inline-block;
}
.pr {
  position: relative;
}
.cc {
  width: 93.6%;
  margin: 0 auto;
}
::-moz-placeholder {
  color: #cacaca;
}
::-webkit-input-placeholder {
  color: #cacaca;
}
:-ms-input-placeholder {
  color: #cacaca;
}

/*基于750px宽度，左右间距为24px,居中显示盒子*/
.flexWrap, .flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.flexItem, .flexi {
  text-align: center;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.tableA {
  display: table;
}
.tableCell {
  display: table-cell;
  vertical-align: middle;
}
.lh {
  line-height: 1;
}
.tdu {
  text-decoration: underline;
}
.vam {
  vertical-align: middle;
}
.wsn {
  white-space: nowrap;
}

/*公用部分*/
.break {
  word-break: break-all;
  word-wrap: break-word
}
#header {
  position: relative;
}

/*font-weight*/
.fwb {
  font-weight: bold;
}
.fwn {
  font-weight: normal;
}
.fwl {
  font-weight: lighter;
}
.ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

/*网站CSS样式库--根据当前实际的项目内容指定的*/

