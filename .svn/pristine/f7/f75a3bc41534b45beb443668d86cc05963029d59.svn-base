.fix-download {
  position: fixed;
  z-index: 99998;
  bottom: 0;
  width: 10.0rem;
  height: 2.0rem;
  background-color: #222;
}
.fix-download .btn-d.btn-down {
  display: block;
  margin: 0.34667rem auto;
  width: 9.2rem;
  height: 1.30667rem;
  line-height: 1.30667rem;
  font-size: 0.48rem;
  text-align: center;
  background: #eaab32;
  color: #333;
  border: none;
  border-radius: 0.10667rem;
}
.icons.icon-downloadapp {
  width: 1.0rem;
  height: 1.0rem;
  background-position: -2.0rem -9.0rem;
}

/*App下载底部浮窗*/
.appDown {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 1.97333rem;
  padding: 0.42667rem 0.4rem 0;
  background: #222;
}
.appDown-in {
  position: relative;
}
.appDown-logo {
  float: left;
  width: 1.12rem;
  height: 1.12rem;
  border-radius: 0.16rem;
  background: #222 url(../image/appDown.png) no-repeat center;
  background-size: 1.12rem 1.12rem;
}
.appDown-txt {
  float: left;
  position: relative;
  margin-left: 0.16rem;
  height: 1.12rem;
  width: 4.66667rem;
  line-height: 1;
  color: #fff;
}
.appDown-txt h3 {
  margin-top: 0.04rem;
  font-size: 0.48rem;
}
.appDown-txt p {
  margin-top: 0.2rem;
  font-size: 0.29333rem;
}
.appDown .btn-down {
  float: right;
  margin-top: 0.09333rem;
  width: 3.0rem;
  height: 0.96rem;
  line-height: 0.96rem;
  font-size: 0.37333rem;
  color: #333;
}
.icons.icon-downloadapp-s {
  width: 0.52rem;
  height: 0.96rem;
  background-position: -2.25333rem -10.01rem;
}
/*App下载底部浮窗*/

/*App顶部浮动广告位窗*/
.h5-appad {
  position: fixed;
  top: 0;
  left: 0;
  padding: 0.13333rem 0.32rem 0;
  height: 1.33333rem;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  z-index: 100;
}
.h5-appad .swiper-container {
  position: relative;
  height: 1.06667rem;
  width: 7.2rem;
  margin: 0;
}
.h5-appad img {
  float: left;
  width: 1.06667rem;
  height: 1.06667rem;
}
.h5-appad .info {
  float: left;
  margin-left: 0.16rem;
  width: 5.6rem;
  font-size: 0.34667rem;
}
.h5-appad .info .title {
}
.h5-appad .info .desc {
}
#swiper-pagination-t {
  position: relative;
}
.h5-appad .swiper-container-horizontal>.swiper-pagination-bullets {
  position: absolute;
  bottom: 0.16rem;
  padding-left: 2.16rem;
  font-size: 0;
  text-align: center;
}
.h5-appad .swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet {
  margin: 0 0.06667rem;
}
.h5-appad .swiper-pagination-bullet {
  width: 0.10667rem;
  height: 0.10667rem;
  display: inline-block;
  border-radius: 100%;
  background: transparent;
  border: 1px solid #f4ad23;
}
.h5-appad .swiper-pagination-bullet-active {
  background-color: #f4ad23;
}
.h5-appad .btn-open {
  position: absolute;
  top: 0.26667rem;
  right: 1.0rem;
  width: 1.33333rem;
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.37333rem;
  text-align: center;
  color: #333;
  background-color: #f4ad23;
  border-radius: 0.12rem;
}
.h5-appad .icons.icon-close {
  display: inline-block;
  vertical-align: middle;
  background-size: 12.0rem 12.0rem;
  background-repeat: no-repeat;
  background-image: url(../image/icons/icons.png?v=0.0.2);

  position: absolute;
  right: 0.32rem;
  top: 0.16rem;
  width: 0.48rem;
  height: 1.0rem;
  background-position: -3.24rem -10.0rem;
}
/*App顶部浮动广告位窗 end */

/*提醒弹窗*/
.remark-box .btn-s {
  height: 30px;
  font-size: 16px;
  line-height: 30px;
  text-align: center;
  border-radius: 15px;
  cursor: pointer;
}
.remark-box .btn-s.primary {
  color: #fff;
  background-color: #f4ad23;
}
.remark-box .btn-s.default{
  color: #1a1a1a;
  background-color: #f4f4f4;
}
.remark-box .btn-s.default:hover {
  color: #f4ad23;
}
.layerbox .remark-box {
  margin-left: -3.06667rem;
  margin-top: -3.2rem;
  width: 6.13333rem;
  font-size: 0.42667rem;
  text-align: center;
  border-radius: 4px;
  background-color: #fff;
  overflow: hidden;
}
.layerbox .remark-box .title {
  position: relative;
  padding: 0 0.32rem;
  height: 0.88rem;
  line-height: 0.88rem;
  font-size: 0.42667rem;
  font-weight: 300;
  color: #fff;
  background-color: #161616;
}
.layerbox .remark-box .title .icon-close {
  position: absolute;
  right: 0.24rem;
  margin-top: 0.13333rem;
  width: 0.6rem;
  height: 0.6rem;
  background-position: -2.2rem -6.12rem;
}
.layerbox .remark-box .content {
  padding: 0.6rem 0.32rem;
  line-height: 0.56rem;
  font-size: 0.37333rem;
  text-align: center;
  color: #666;
  background-color: #fff;
}
.layerbox .remark-box .btn-group {
  padding: 0 0.4rem 0.32rem;
  background-color: #fff;
}
.layerbox .remark-box .btn-group .btn-s {
  display: inline-block;
  font-size: 0.4rem;
  width: 1.6rem;
  height: 0.6rem;
  line-height: 0.6rem;
  color: #989898;
  background-color: #f4f4f4;
}
.layerbox .remark-box .btn-group .btn-s:nth-of-type(2) {
  margin-left: 12px;
  color: #1a1a1a;
  background-color: #f4ad23;
}
/*提醒弹窗end*/