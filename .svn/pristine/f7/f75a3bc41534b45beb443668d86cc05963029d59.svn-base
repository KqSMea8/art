/*弹框提示*/
.tipshow {
  min-width: 5.0rem;
  min-height: 1.5625rem;
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -2.5rem;
  margin-top: -0.78125rem;
  -webkit-border-radius: 0.0625rem;
  border-radius: 0.0625rem;
  z-index: 100000;
}
.tipshow .tipbg {
  background-color: rgba(0, 0, 0, 0.8);
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
  -webkit-border-radius: 0.0625rem;
  border-radius: 0.0625rem;
}
.tipshow .fontwrap {
  display: table;
  width: 100%;
  min-height: 1.5625rem;
  height: 1.5625rem;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.tipshow .font {
  color: #fff;
  font-size: 0.375rem;
  line-height: 0.53125rem;
  position: relative;
  text-align: center;
  display: table-cell;
  vertical-align: middle;
  padding: 0px 0.46875rem;
}
.tipshow.effect {
  -webkit-animation: fadeIn 1s 0s ease both;
  -moz-animation: fadeIn 1s 0s ease both;
}
@-webkit-keyframes fadeIn {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}
@-moz-keyframes fadeIn {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}

/*/弹框提示*/

/*头像*/
.avatar {
  border-radius: 50%;
}

/*公用部分 end*/

/* CSS3动画 */
.animated {
  -webkit-animation-duration: .8s;
  animation-duration: .8s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both
}

/*fadeInUp*/
@-webkit-keyframes fadeInUp {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    -ms-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0)
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
@keyframes fadeInUp {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    -ms-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0)
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.fadeInUp {
  -webkit-animation-name: fadeInUp;
  -moz-animation-name: fadeInUp;
  -ms-animation-name: fadeInUp;
  animation-name: fadeInUp
}

/*fadeInUp end*/
@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
.fadeOut {
  -webkit-animation-name: fadeOut;
  animation-name: fadeOut;
}
@keyframes fadeInAndOut {
  0% {
    opacity: 0;
  }
  25% {
    opacity: 1;
  }
  75% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
@-webkit-keyframes fadeInAndOut {
  0% {
    opacity: 0;
    display: none;
  }
  25% {
    opacity: 1;
    display: block;
  }
  75% {
    opacity: 1;
    display: block;
  }
  100% {
    opacity: 0;
    display: none;
  }
}
.fadeInAndOut {
  -webkit-animation-name: fadeInAndOut;
  animation-name: fadeInAndOut;
}

/* 弹出动画 */
@-webkit-keyframes anim-scale {

  /* 默认 */
  0% {
    opacity: 0;
    -webkit-transform: scale(.5);
    transform: scale(.5)
  }
  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}
@keyframes anim-scale {

  /* 默认 */
  0% {
    opacity: 0;
    -webkit-transform: scale(.5);
    transform: scale(.5)
  }
  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}
.anim-scale {
  animation-name: anim-scale;
  -webkit-animation-name: anim-scale;
}
@-webkit-keyframes anim-up {
  0% {
    opacity: 0;
    -webkit-transform: translateY(25.0rem);
    transform: translateY(25.0rem)
  }
  100% {
    opacity: 1;
    -webkit-transform: translateY(0);
    transform: translateY(0)
  }
}
@keyframes anim-up {
  0% {
    opacity: 0;
    -webkit-transform: translateY(25.0rem);
    transform: translateY(25.0rem)
  }
  100% {
    opacity: 1;
    -webkit-transform: translateY(0);
    transform: translateY(0)
  }
}
.anim-up {
  -webkit-animation-name: anim-up;
  animation-name: anim-up
}

/* 弹出动画 end */

/* 放大缩小动画 */
@-webkit-keyframes anim-zooming {

  /* 默认 */
  0% {
    -webkit-transform: scale(1);
    transform: scale(1)
  }
  50% {
    -webkit-transform: scale(1.5);
    transform: scale(1.5)
  }
  100% {
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}
@keyframes anim-zooming {

  /* 默认 */
  0% {
    -webkit-transform: scale(1);
    transform: scale(1)
  }
  25% {
    -webkit-transform: scale(1.5);
    transform: scale(1.5)
  }
  50% {
    -webkit-transform: scale(1);
    transform: scale(1)
  }
  75% {
    -webkit-transform: scale(0.6);
    transform: scale(0.6)
  }
  100% {
    -webkit-transform: scale(1);
    transform: scale(1)
  }
}
.anim-zooming {
  animation-name: anim-zooming;
  -webkit-animation-name: anim-zooming;
  animation-duration: 0.6s;
  -webkit-animation-duration: 0.6s;
}
/* 放大缩小动画 end */

