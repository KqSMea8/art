/*element reset*/
.el-form-item.is-required .el-form-item__label:before {
  content: '';
}
.el-form-item__label {
  font-size: 16px;
  color: #333;
  line-height: 48px;
  height: 48px;
  padding: 0;
  padding-right: 12px;
}
.el-input {
  font-size: 16px;
}
.el-button {
  display: inline-block;
  height: 48px;
  text-align: center;
  line-height: 1;
  white-space: nowrap;
  cursor: pointer;
  background: #fff;
  border: 1px solid rgb(217, 211, 191);
  color: rgb(61, 54, 31);
  -webkit-appearance: none;
  text-align: center;
  box-sizing: border-box;
  outline: 0;
  margin: 0;
  padding: 10px 15px;
  font-size: 16px;
  border-radius: 2px;
}
.el-button--primary {
  color: #141414;
  background-color: #f4ad23;
  border-color: #f4ad23;
}
.el-button--primary.is-active,
.el-button--primary:active,
.el-button--primary:focus,
.el-button--primary:hover {
  color: #141414;
}
.el-button.btn-14.clicked {
  color: #fff;
  background-color: #999;
  border-color: #999;
}
.input-m.el-input,
.input-m.el-input input {
  width: 202px;
} 
.el-input__inner {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  background-color: #fff;
  background-image: none;
  border-radius: 0;
  border: 1px solid #dcdcdc;
  box-sizing: border-box;
  color: #333;
  display: block;
  font-size: 16px;
  height: 48px;
  line-height: 1;
  outline: 0;
  padding: 3px 10px;
  transition: border-color .2s cubic-bezier(.645,.045,.355,1);
  width: 100%;
}
.el-form-item {
  margin-bottom: 30px;
}

.error-text {
  height: 20px;
  line-height: 20px;
  font-size: 14px;
  text-align: center;
  color: #ff4949;
}

.el-textarea__inner {
  padding: 12px;
  font-size: 16px;
  line-height: 1.5;
  color: #555;
  background-color: #fff;
  border-radius: 0;
}



.icons {
  display: inline-block;
  background-repeat: no-repeat;
  -webkit-background-size: 1000px 1000px;
  background-size: 1000px 1000px;
  background-image: url(../image/icons/pc-icons.png);
}
.btn-group {
  text-align: center;
}
.btn {
  height: 48px;
  font-size: 16px;
  line-height: 48px;
  color: #141414;
  background-color: #f4ad23;
  border-radius: 2px;
  cursor: pointer;
}
.btn-s:hover,.btn:hover {
  transition: background-position 0.1s linear 0s;
  box-shadow: 0 1px 2px 0 #bbb;
}
.btn-24 {
  width: 240px;
}

.btn-14 {
  width: 140px;
}

.btn-166 {
  width: 166px;
}
.btn-250 {
  width: 250px;
}

.btn-14.clicked {
  color: #fff;
  background-color: #999;
  border-color: #999;
}

/*输入框&文本区域*/
.input {
  display: block;
  width: 100%;
  height: 48px;
  padding: 6px 12px;
  font-size: 18px;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text {
  display: block;
  width: 100%;
  padding: 12px;
  font-size: 16px;
  line-height: 1.5;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdcdc;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
  -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.text:focus,.input:focus {
  border-color: #f4ad23;
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(244,173,35,.6);
}
/*输入框&文本区域 end*/

