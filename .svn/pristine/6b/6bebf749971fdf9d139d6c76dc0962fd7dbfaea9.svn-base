.gole{
  -webkit-animation: goout 0.5s ease-out;
          animation: goout 0.5s ease-out;
  -webkit-transform: translateY(0.56rem);
          transform: translateY(0.56rem);
}
@-webkit-keyframes goout{
  from {
    -webkit-transform: translateY(2.67rem);
            transform: translateY(2.67rem);
  }
  to {
    -webkit-transform: translateY(0.56rem);
            transform: translateY(0.56rem);
  }
}
@keyframes goout{
  from {
    -webkit-transform: translateY(2.67rem);
            transform: translateY(2.67rem);
  }
  to {
    -webkit-transform: translateY(0.53rem);
            transform: translateY(0.53rem);
  }
}
.boxbtn{
  width: 6.74rem;
  height: 2.62rem;
  background: #fff;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: absolute;
  z-index: 7;
  left: 0.4rem;
  bottom: 0.6rem;
  margin-left:-0.03rem;
}
.boxbtn:after{
  position: absolute;
  left: 0rem;
  top: -0.03rem;
  z-index: 10;
  margin-left: -0.03rem;
  content: '';
  width: 0;
  height: 0;
  border-bottom: 0.88rem solid #e33d3d;
  border-right: 3.2rem solid transparent;
}
.boxbtn:before{
  position: absolute;
  right: 0rem;
  top: -0.03rem;
  margin-right: -0.03rem;
  z-index: 10;
  content: '';
  width: 0;
  height: 0;
  border-bottom: 0.88rem solid #e33d3d;
  border-left: 3.2rem solid transparent;
}

