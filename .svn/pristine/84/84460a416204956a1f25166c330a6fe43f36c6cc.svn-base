  <div id="tab-bar">
    <nav class="tab-bar-list">
      <a class="tab-bar-item" href="/H5/Index/index">
        <i class="icons icon-recommend"></i>
        <span class="tab-label">推荐</span>
      </a>
      <a class="tab-bar-item active" href="/H5/Gallery/index">
        <i class="icons icon-gallery"></i>
        <span class="tab-label">画廊</span>
      </a>
      <a class="tab-bar-item" href="/H5/User/index">
        <i class="icons icon-user"></i>
        <span class="tab-label">我</span>
      </a>
    </nav>
  </div>
  <script type="text/javascript" src="/Public/H5/js/gallery/gallery.js"></script>
  <script type="text/javascript">
    $(document).ready(function () {
      fadeInUp('.gallery-list', '.gallery-item', 0.2);
    });
  </script>
</body>
</html>